          <includedValues>SNOMED-CT::394602003::Rehabilitation - speciality</includedValues>
          <includedValues>SNOMED-CT::74964007::Other</includedValues>
        </constraint>
      </Rule>
      <Rule path="/description[at0001]/items[at0060]" max="0" />
