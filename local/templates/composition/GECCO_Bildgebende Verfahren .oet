  <definition xsi:type="COMPOSITION" archetype_id="openEHR-EHR-COMPOSITION.registereintrag.v1" concept_name="Registereintrag" name="Bildgebende Verfahren">
    <Content xsi:type="ACTION" archetype_id="openEHR-EHR-ACTION.procedure.v1" concept_name="Prozedur" max="1" path="/content">
      <Rule path="/description[at0001]/items[at0002]">
        <constraint xsi:type="textConstraint">
          <includedValues>SNOMED-CT::77477000::Computerized axial tomography (procedure)</includedValues>
          <includedValues>SNOMED-CT::168537006::Plain radiography</includedValues>
          <includedValues>SNOMED-CT::16310003::Diagnostic ultrasonography (procedure)</includedValues>
        </constraint>
      </Rule>
      <Rule path="/description[at0001]/items[at0049]" name="Befund Bildgebender Verfahren im Rahmen von Covid-19">
        <constraint xsi:type="textConstraint">
          <includedValues>Unspezifischer Befund</includedValues>
          <includedValues>Covid-typischer Befund</includedValues>
