                            queryName="ValueSet/$expand?url=http://fhir.de/ValueSet/dkgev/Fachabteilungsschluessel-erweitert" terminologyID="//fhir.hl7.org/"/>
			        </constraint>
                </Rule>
            </Items>
        </Context>
        <Rule path="/context/other_context[at0001]/items[at0005]">
            <constraint limitToList="false" xsi:type="tem:textConstraint">
                <termQueryId
                    queryName="ValueSet/$expand?url=http://fhir.de/ValueSet/kontaktebene-de" terminologyID="//fhir.hl7.org/"/>
            </constraint>
        </Rule>
        <Rule path="/context/other_context[at0001]/items[at0004]">
            <constraint limitToList="false" xsi:type="tem:textConstraint">
                <includedValues>local_terms::AMB::Ambulant (ambulatory)</includedValues>
                <includedValues>local_terms::PRENC::Prästationär (pre-admission)</includedValues>
                <includedValues>local_terms::VR::Virtuell (virtual)</includedValues>
                <includedValues>local_terms::HH::Stationsäquivalent (home health)</includedValues>
            </constraint>
        </Rule>
        <Rule path="/context/other_context[at0001]/items[at0010]">
