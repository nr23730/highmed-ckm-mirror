                <Rule max="1" min="1" path="/data[at0001]/events[at0002]"/>
                <Rule max="0" path="/data[at0001]/events[at0002]/data[at0003]/items[at0034]"/>
                <Rule max="1" min="1" path="/data[at0001]/events[at0002]/data[at0003]/items[at0022]"/>
