                    <constraint xsi:type="tem:textConstraint">
                        <includedValues>Behandlung auf der Intensivstation</includedValues>
                    </constraint>
                </Rule>
                <Rule min="1"
                    name="Wird/Wurde die Aktivität durchgeführt?" path="/data[at0001]/events[at0002]/data[at0003]/items[at0022]/items[at0005]">
