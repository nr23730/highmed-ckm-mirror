      <Rule path="/data[at0001]/items[at0013]">
        <constraint xsi:type="textConstraint">
          <includedValues>01xx (Krankkenhausbehandlung, vollstationär)</includedValues>
          <includedValues>02xx Krankkenhausbehandlung, vollstationär mit vorausgegangeer vorstationärer Behandlung</includedValues>
          <includedValues>03xx Krankkenhausbehandlung, teilstationär</includedValues>
          <includedValues>04xx vorstationäre Behandlung ohne anschließende vollstationäre Behandlung</includedValues>
          <includedValues>05xx stationäre Entbindung</includedValues>
          <includedValues>06xx Geburt</includedValues>
          <includedValues>08xx Stationäre Aufnahme zur Organentnahme</includedValues>
          <includedValues>10xx Stationsäquivalente Behandlung</includedValues>
        </constraint>
      </Rule>
