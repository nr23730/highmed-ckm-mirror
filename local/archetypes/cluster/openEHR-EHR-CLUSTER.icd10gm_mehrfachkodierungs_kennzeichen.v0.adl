definition
	CLUSTER[at0000] matches {    -- ICD10gm_multiple_coding_marker
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] occurrences matches {0..1} matches {    -- Code marker
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0002,    -- +
							at0003,    -- *
							at0004]    -- !
						}
					}
				}
			}
		}
	}


