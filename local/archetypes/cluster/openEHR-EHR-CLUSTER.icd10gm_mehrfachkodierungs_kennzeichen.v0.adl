		["de"] = <
			language = <[ISO_639-1::de]>
			purpose = <"Zur Erfassung mehrerer Kodierungsmarker für ICD-10-gm-Kodes nach dem Stern-Kreuz-System für ein bestimmtes Problem oder eine bestimmte Diagnose. ">
			use = <"Verwenden Sie diesen Archetyp, um mehrere Kodierungsmarker für Diagnosen in ICD10-gm-Terminologie zu erfassen.

Dieser Archetyp ist dafür vorgesehen, im Archetyp EVALUATION.problem_diagnosis in \"Specific details\" SLOT aufgenommen zu werden.">
			keywords = <"icd10gm, mehrfachcodierung, diagnose", ...>
			misuse = <"Darf nicht zur Beschreibung von Mehrfachkodierungskennzeichen für andere Terminologien als ICD10-gm verwendet werden.">
			copyright = <"© HiGHmed">
		>
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record multiple coding markers for ICD-10-gm codes according to the \"cross-star\" coding approach for a specified problem or diagnosis.">
			use = <"Use to record multiple coding markers for diagnoses in ICD10-gm terminology.

This archetype is designed to be included in Specific details SLOT in the EVALUATION.problem_diagnosis archetype.">
			keywords = <"icd10gm,multiple coding,diagnosis", ...>
			misuse = <"Not to be used to describe multiple coding indicators for terminologies other than ICD10-gm.">
