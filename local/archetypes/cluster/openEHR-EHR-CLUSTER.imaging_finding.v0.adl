definition
	CLUSTER[at0000] matches {    -- Imaging finding
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0024] matches {    -- Finding
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0001] occurrences matches {0..1} matches {    -- Presence?
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0035,    -- Present
							at0036,    -- Absent
							at0037]    -- Indeterminate
						}
					}
				}
			}
			ELEMENT[at0028] occurrences matches {0..1} matches {    -- Description
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0033] occurrences matches {0..1} matches {    -- Body site
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0034] occurrences matches {0..*} matches {    -- Structured body site
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location\.v1/}
			}
			allow_archetype CLUSTER[at0014] occurrences matches {0..*} matches {    -- Additional details
				include
					archetype_id/value matches {/.*/}
			}
			ELEMENT[at0003] occurrences matches {0..*} matches {    -- Comment
				value matches {
					DV_TEXT matches {*}
				}
			}
		}
	}

