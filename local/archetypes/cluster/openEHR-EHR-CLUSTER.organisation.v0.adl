	openEHR-EHR-CLUSTER.organisation.v0

concept
	[at0000]	-- Organisation
language
	original_language = <[ISO_639-1::en]>
description
	original_author = <
		["name"] = <"Heather Leslie">
		["organisation"] = <"Atomica Informatics">
		["email"] = <"heather.leslie@atomicainformatics.com">
		["date"] = <"2021-04-14">
	>
	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record details related to naming or contacting an organisation within the context of a health record.">
			use = <"Use to record details related to naming or contacting an organisation, including a contact person, within the context of a health record.

The intended scope of this archetype is to provide simple details about the name or contact details for an organisation, or contact person in an organisation, within the context of a component of the health record, extract or external communication For example: naming a care team member and their contact details in a referral.">
			keywords = <"organisation, provider, carer", ...>
			misuse = <"Not to be used to represent or replace the formal demographic details which require additional details such as alternative names and identifiers for formal identification management. It is recommended that an external Healthcare Provider Index or archetypes based on the openEHR Demographic Information Model be used for this purpose.">
			copyright = <"© openEHR Foundation">
		>
	>
	lifecycle_state = <"in_development">
