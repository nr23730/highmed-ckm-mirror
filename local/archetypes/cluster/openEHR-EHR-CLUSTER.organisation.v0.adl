definition
	CLUSTER[at0000] matches {    -- Organisation
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0002] occurrences matches {0..1} matches {    -- Name line
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0001] occurrences matches {0..*} matches {    -- Identifier
				value matches {
					DV_IDENTIFIER matches {*}
				}
			}
			ELEMENT[at0004] occurrences matches {0..*} matches {    -- Role
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0003] occurrences matches {0..*} matches {    -- Contact person
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.person\.v0/}
			}
			CLUSTER[at0005] occurrences matches {0..*} matches {    -- Address
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0006] occurrences matches {0..1} matches {    -- Type
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0007,    -- Business
									at0008,    -- Postal
									at0010]    -- Temporary
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0011] occurrences matches {0..*} matches {    -- Address line
						value matches {
							DV_TEXT matches {*}
						}
						name matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0012,    -- Address line 1
									at0013,    -- Address line 2
									at0014,    -- Address line 3
									at0015]    -- Address line 4
								}
							}
						}
					}
					allow_archetype CLUSTER[at0016] occurrences matches {0..*} matches {    -- Structured address
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.structured_address\.v0/}
					}
					ELEMENT[at0017] occurrences matches {0..*} matches {    -- Suburb/Town/City/Locality
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0018] occurrences matches {0..*} matches {    -- District/County
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0019] occurrences matches {0..1} matches {    -- State/Territory/Province
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0020] occurrences matches {0..1} matches {    -- Postcode
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0021] occurrences matches {0..1} matches {    -- Country
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0022] occurrences matches {0..1} matches {    -- Postal delivery point
						value matches {
							DV_IDENTIFIER matches {*}
						}
					}
					ELEMENT[at0023] occurrences matches {0..1} matches {    -- Valid from
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0024] occurrences matches {0..1} matches {    -- Valid to
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
				}
			}
			CLUSTER[at0025] occurrences matches {0..*} matches {    -- Electronic communication
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0026] occurrences matches {0..1} matches {    -- Medium
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0026,    -- Medium
									at0027,    -- Telephone (excluding mobile telephone)
									at0028,    -- Email
									at0029]    -- Pager
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0030] occurrences matches {0..1} matches {    -- Type
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0034] matches {    -- Value
						value matches {
							DV_TEXT matches {*}
							DV_URI matches {*}
						}
					}
					ELEMENT[at0035] occurrences matches {0..1} matches {    -- Valid from
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0036] occurrences matches {0..1} matches {    -- Valid to
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
				}
			}
			allow_archetype CLUSTER[at0037] occurrences matches {0..*} matches {    -- Additional details
				include
					archetype_id/value matches {/.*/}
			}
		}
	}

ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Organisation">
					description = <"A record of details related to naming or contacting an organisation.">
					comment = <"For example: a clinician working as part of a care team; a relative; or a care-giver.">
				>
				["at0001"] = <
					text = <"Identifier">
					description = <"Identifier associated with the identified organisation.">
				>
				["at0016"] = <
					text = <"Structured address">
					description = <"Additional structured low level geographical/physical details about a location that can be concatenated to form one or more 'Address line' entries.">
				>
				["at0037"] = <
					text = <"Additional details">
					description = <"Additional details about the organisation.">
				>
				["at0004"] = <
					text = <"Role">
					description = <"The role or capacity in which the organisation contributes to the health or social care of the subject of care.">
				>
				["at0002"] = <
					text = <"Name line">
					description = <"The name by which an organisation is known.">
				>
				["at0003"] = <
					text = <"Contact person">
					description = <"Details about a contact person, or people, within the organisation.">
				>
				["at0005"] = <
					text = <"Address">
					description = <"Details about a specific type of address for the organisation.">
				>
				["at0023"] = <
					text = <"Valid from">
					description = <"The start of the period for which this address is valid.">
				>
				["at0024"] = <
					text = <"Valid to">
					description = <"The end of the period for which this address is valid.">
				>
				["at0006"] = <
					text = <"Type">
					description = <"The purpose or use for an address.">
				>
				["at0007"] = <
					text = <"Business">
					description = <"Address of the physical location of a business or office.">
				>
				["at0008"] = <
					text = <"Postal">
					description = <"Address for sending correspondence.">
				>
				["at0010"] = <
					text = <"Temporary">
					description = <"Temporary address for an organisation.">
				>
				["at0011"] = <
					text = <"Address line">
					description = <"A text composite of one or more structured address components that represent a low level of geographical/physical description of a location that, used in conjunction with the other high-level  address components i.e. ‘Suburb/Town/Locality’, ‘Postcode’, and ‘State/Territory/Province’, forms a complete geographical/physical address.">
					comment = <"Run-time name constraints allow for multiple named 'Address lines', as required.
For example: 52 Davis Street; Level 2; Upper ground; Treasury building; Australia House; Apartment 7A.">
				>
				["at0012"] = <
					text = <"Address line 1">
					description = <"">
				>
				["at0013"] = <
					text = <"Address line 2">
					description = <"">
				>
				["at0014"] = <
					text = <"Address line 3">
					description = <"">
				>
				["at0015"] = <
					text = <"Address line 4">
					description = <"">
				>
				["at0017"] = <
					text = <"Suburb/Town/City/Locality">
					description = <"The name of the general locality containing the address.">
					comment = <"Coding with an external terminology is preferred, where possible. For example: Fitzroy, Calgary.">
				>
				["at0018"] = <
					text = <"District/County">
					description = <"The name of an internal political or geographic district or area within a state, territory or province containing the address.">
					comment = <"Coding with an external terminology is preferred, where possible. ">
				>
				["at0019"] = <
					text = <"State/Territory/Province">
					description = <"The name of an internal political or geographic division of a country containing the address.">
					comment = <"Coding with an external terminology is preferred, where possible. For example: Victoria; Alberta.">
				>
				["at0020"] = <
					text = <"Postcode">
					description = <"The code for a postal delivery area containing the address, aligned with locality, suburb or place for an address, as defined by the relevant postal delivery service.">
					comment = <"Also known as ZIP code.">
				>
				["at0021"] = <
					text = <"Country">
					description = <"The name of the country containing the address.">
					comment = <"Coding with an external terminology is preferred, where possible. For example: Australia; Canada">
				>
				["at0022"] = <
					text = <"Postal delivery point">
					description = <"A unique number assigned to a postal delivery point, as defined by the relevant postal delivery service.">
				>
				["at0026"] = <
					text = <"Medium">
					description = <"The type of communication mechanism or channel.">
					comment = <"The scope of electronic communication medium includes, but is not limited to use of : a telephone, cellular phone, computer, or pager.">
				>
				["at0027"] = <
					text = <"Telephone (excluding mobile telephone)">
					description = <"">
				>
				["at0028"] = <
					text = <"Email">
					description = <"">
				>
				["at0029"] = <
					text = <"Pager">
					description = <"">
				>
				["at0030"] = <
					text = <"Type">
					description = <"The purpose or use for the identified medium.">
					comment = <"Coding with an external terminology is preferred, where possible.">
				>
				["at0034"] = <
					text = <"Value">
					description = <"The unique combination of alphnumeric characters, meaningful for the identified medium, for contacting the organisation.">
					comment = <"For example: area code + landline phone/pager number; country code + mobile phone number; email address; or skype contact name.">
				>
				["at0025"] = <
					text = <"Electronic communication">
					description = <"Details about a specific type of electronic communication for the organisation.">
				>
				["at0035"] = <
					text = <"Valid from">
					description = <"The start of the period for which this medium and type is valid.">
				>
				["at0036"] = <
					text = <"Valid to">
					description = <"The end of the period for which this medium and type is valid.">
				>
			>
		>
	>
