definition
	CLUSTER[at0000] matches {	-- Wohnsituation nach Nikolaus
		items cardinality matches {1..*; unordered} matches {
			CLUSTER[at0039] occurrences matches {0..1} matches {	-- Treppen
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0047] occurrences matches {0..1} matches {	-- Viele Treppen, erster Stock oder höher
						value matches {0|[local::at0048]}		-- Ja
					}
					ELEMENT[at0046] occurrences matches {0..1} matches {	-- Wohnung im Erdgeschoss oder Lift im Haus
						value matches {1|[local::at0049]}		-- Ja
					}
				}
			}
			CLUSTER[at0040] occurrences matches {0..1} matches {	-- Komfort
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0050] occurrences matches {0..1} matches {	-- Wohnung eingeschossig, geräumig und rollstuhlgängig
						value matches {1|[local::at0051]}		-- Ja
					}
					ELEMENT[at0052] occurrences matches {0..1} matches {	-- Beengte Verhältnisse, Türschwellen, viele Teppiche
						value matches {0|[local::at0053]}		-- Ja
					}
					ELEMENT[at0054] occurrences matches {0..1} matches {	-- Mehrere Wohnebenen, nicht rollstuhlgeeignet
						value matches {0|[local::at0055]}		-- Ja
					}
				}
			}
			CLUSTER[at0041] occurrences matches {0..1} matches {	-- Heizung
