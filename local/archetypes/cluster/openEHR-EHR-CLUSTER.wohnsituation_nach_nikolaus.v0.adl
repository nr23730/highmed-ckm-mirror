			CLUSTER[at0044] occurrences matches {0..1} matches {	-- Telefon
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0068] occurrences matches {0..1} matches {	-- Vorhanden
						value matches {1|[local::at0074]}		-- Ja
					}
					ELEMENT[at0067] occurrences matches {0..1} matches {	-- Nicht Vorhanden
						value matches {0|[local::at0073]}		-- Ja
					}
				}
			}
			CLUSTER[at0045] occurrences matches {0..1} matches {	-- Beleuchtung
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0062] occurrences matches {0..1} matches {	-- Treppenhaus und Flure hell, genügend Lichtschalter
						value matches {1|[local::at0075]}		-- Ja
					}
					ELEMENT[at0063] occurrences matches {0..1} matches {	-- Treppenhaus und Flure schummrig beleuchtet
						value matches {0|[local::at0076]}		-- Ja
					}
					ELEMENT[at0064] occurrences matches {0..1} matches {	-- Wenig Lichtschalter
						value matches {0|[local::at0077]}		-- Ja
					}
				}
			}
			CLUSTER[at0078] occurrences matches {0..1} matches {	-- Einkaufen
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0086] occurrences matches {0..1} matches {	-- Alle Geschäfte des tgl. Bedarfs leicht erreichbar
						value matches {1|[local::at0092]}		-- Ja
					}
					ELEMENT[at0085] occurrences matches {0..1} matches {	-- Nur Bäcker/Metzger in der Nähe
						value matches {0|[local::at0093]}		-- Ja
					}
					ELEMENT[at0084] occurrences matches {0..1} matches {	-- Alle Geschäfte weiter entfernt
						value matches {0|[local::at0094]}		-- Ja
					}
				}
			}
			CLUSTER[at0079] occurrences matches {0..1} matches {	-- Nahverkehr
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0083] occurrences matches {0..1} matches {	-- Haltestelle in der Nähe (<1km)
						value matches {1|[local::at0096]}		-- Ja
					}
					ELEMENT[at0082] occurrences matches {0..1} matches {	-- Nächste Haltestelle weiter entfernt
						value matches {0|[local::at0095]}		-- Ja
					}
				}
			}
			CLUSTER[at0080] occurrences matches {0..1} matches {	-- Wohndauer
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0087] occurrences matches {0..1} matches {	-- Wohnt schon lange Zeit in der Wohnung (>5 Jahre)
						value matches {1|[local::at0098]}		-- Ja
					}
					ELEMENT[at0088] occurrences matches {0..1} matches {	-- Hat innerhalb der ltz. 5 Jahre Wohnung bezogen
						value matches {0|[local::at0097]}		-- Ja
					}
				}
			}
			CLUSTER[at0081] occurrences matches {0..1} matches {	-- Wohlbefinden in der Wohnung/Wohngegend
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0089] occurrences matches {0..1} matches {	-- Bin mit der Wohnsituation sehr zufrieden
						value matches {1|[local::at0099]}		-- Ja
					}
					ELEMENT[at0090] occurrences matches {0..1} matches {	-- Geht so, muss zufrieden sein
						value matches {0|[local::at0100]}		-- Ja
					}
					ELEMENT[at0091] occurrences matches {0..1} matches {	-- Bin unzufrieden
						value matches {0|[local::at0101]}		-- Ja
					}
				}
			}
			ELEMENT[at0038] occurrences matches {0..1} matches {	-- Zwischensumme Wohnsituation
				value matches {
					DV_COUNT matches {
						magnitude matches {|0..11|}
					}
				}
			}
		}
	}


