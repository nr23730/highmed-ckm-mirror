definition
	CLUSTER[at0000] matches {    -- Multiple_coding_ICD-10-GM
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] occurrences matches {0..1} matches {    -- Multiple coding identifier
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0002,    -- †
							at0003,    -- *
							at0004]    -- !
						}
					}
				}
			}
		}
	}


