definition
	CLUSTER[at0000] matches {    -- Evidenz Kodierung
		items cardinality matches {1..*; unordered} matches {
			CLUSTER[at0001] occurrences matches {0..1} matches {    -- Evidenz Code
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {    -- Evidenz Codesystem
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0003,    -- ACMG
									at0004,    -- AMP
									at0005,    -- CAP
									at0006,    -- ENIGMA
									at0007,    -- InSiGHT
									at0008,    -- HiGHmed
									at0009,    -- ZPM
									at0010]    -- CPIC
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0011] occurrences matches {0..1} matches {    -- Version
						value matches {
							DV_COUNT matches {*}
						}
					}
					CLUSTER[at0012] occurrences matches {0..1} matches {    -- Evidenzlevel
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0013] occurrences matches {0..1} matches {    -- Evidenzlevel
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0014] occurrences matches {0..*} matches {    -- Zusatzverweise
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0015,    -- Is
											at0016,    -- Iv
											at0017,    -- Z
											at0018]    -- R
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}


