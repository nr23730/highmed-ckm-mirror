									1|[local::at0018],
									2|[local::at0019],
									3|[local::at0020],
									4|[local::at0021],
									5|[local::at0022]
								}
							}
							ELEMENT[at0008] occurrences matches {0..1} matches {    -- dass Sie mit anderen Menschen aufgrund Ihrer Kaufgewohnheiten Probleme haben?
								value matches {
									0|[local::at0017],
									1|[local::at0018],
									2|[local::at0019],
									3|[local::at0020],
									4|[local::at0021],
									5|[local::at0022]
								}
							}
							ELEMENT[at0009] occurrences matches {0..1} matches {    -- dass Sie unter Ihren Kaufgewohnheiten leiden?
