							}
							ELEMENT[at0010] occurrences matches {0..1} matches {    -- dass Sie durch Ihr Kaufverhalten unter finanziellen Problemen leiden?
								value matches {
									0|[local::at0017],
									1|[local::at0018],
									2|[local::at0019],
									3|[local::at0020],
									4|[local::at0021],
									5|[local::at0022]
								}
							}
							ELEMENT[at0011] occurrences matches {0..1} matches {    -- dass es Ihnen unangenehm ist, wenn andere Sie auf Ihr Kaufverhalten ansprechen?
								value matches {
									0|[local::at0017],
									1|[local::at0018],
									2|[local::at0019],
									3|[local::at0020],
									4|[local::at0021],
									5|[local::at0022]
								}
							}
							ELEMENT[at0012] occurrences matches {0..1} matches {    -- dass es Ihnen schlecht geht und sich das bessert, wenn Sie einkaufen?
								value matches {
									0|[local::at0017],
									1|[local::at0018],
									2|[local::at0019],
									3|[local::at0020],
									4|[local::at0021],
									5|[local::at0022]
								}
							}
							ELEMENT[at0013] occurrences matches {0..1} matches {    -- dass Sie ständig ans Kaufen denken müssen?
								value matches {
									0|[local::at0017],
									1|[local::at0018],
									2|[local::at0019],
									3|[local::at0020],
									4|[local::at0021],
									5|[local::at0022]
								}
							}
						}
					}
					CLUSTER[at0003] occurrences matches {0..1} matches {    -- Faktor 2: Exzessives Kaufverhalten
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0014] occurrences matches {0..1} matches {    -- dass Sie mehr kaufen, als Sie sich vorgenommen haben?
								value matches {
									0|[local::at0017],
									1|[local::at0018],
									2|[local::at0019],
									3|[local::at0020],
									4|[local::at0021],
									5|[local::at0022]
								}
							}
							ELEMENT[at0015] occurrences matches {0..1} matches {    -- dass Sie mehr Dinge kaufen als Sie benötigen?
								value matches {
									0|[local::at0017],
									1|[local::at0018],
									2|[local::at0019],
									3|[local::at0020],
									4|[local::at0021],
									5|[local::at0022]
								}
							}
							ELEMENT[at0016] occurrences matches {0..1} matches {    -- dass Sie länger einkaufen als beabsichtigt?
								value matches {
									0|[local::at0017],
									1|[local::at0018],
									2|[local::at0019],
									3|[local::at0020],
									4|[local::at0021],
									5|[local::at0022]
								}
							}
						}
					}
				}
			}
		}
	}


