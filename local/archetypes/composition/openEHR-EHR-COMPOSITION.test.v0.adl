definition
	COMPOSITION[at0000] matches {	-- The Pathological Buying Screener
		category matches {
			DV_CODED_TEXT matches {
				defining_code matches {[openehr::433]}
			}
		}
		context matches {
			EVENT_CONTEXT matches {
				other_context matches {
					ITEM_TREE[at0004] matches {	-- Baum
						items cardinality matches {0..*; unordered} matches {
							CLUSTER[at0010] occurrences matches {0..1} matches {	-- Wie oft kommt es vor...
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0015] occurrences matches {0..1} matches {	-- dass Sie ständig ans Kaufen denken müssen?
										value matches {
											1|[local::at0016], 	-- Sehr häufig
											2|[local::at0017], 	-- Häufig
											3|[local::at0018], 	-- Teils/Teils
											4|[local::at0019], 	-- Weniger häufig
											5|[local::at0020]  	-- Gar nicht
										}
									}
									ELEMENT[at0024] occurrences matches {0..1} matches {	-- dass es Ihnen unangenehm ist, wenn andere Sie auf Ihr Kaufverhalten ansprechen
										value matches {
											1|[local::at0016], 	-- Sehr häufig
											2|[local::at0017], 	-- Häufig
											3|[local::at0018], 	-- Teils/Teils
											4|[local::at0019], 	-- Weniger häufig
											5|[local::at0020]  	-- Gar nicht
										}
									}
									ELEMENT[at0025] occurrences matches {0..1} matches {	-- dass Sie durch Ihr Kaufverhalten unter finanziellen Problemen leiden?
										value matches {
											1|[local::at0016], 	-- Sehr häufig
											2|[local::at0017], 	-- Häufig
											3|[local::at0018], 	-- Teils/Teils
											4|[local::at0019], 	-- Weniger häufig
											5|[local::at0020]  	-- Gar nicht
										}
									}
									ELEMENT[at0026] occurrences matches {0..1} matches {	-- dass Sie länger einkaufen als beabsichtigt?
										value matches {
											1|[local::at0016], 	-- Sehr häufig
											2|[local::at0017], 	-- Häufig
											3|[local::at0018], 	-- Teils/Teils
											4|[local::at0019], 	-- Weniger häufig
											5|[local::at0020]  	-- Gar nicht
										}
									}
									ELEMENT[at0027] occurrences matches {0..1} matches {	-- dass Sie unter Ihren Kaufgewohnheiten leiden?
										value matches {
											1|[local::at0016], 	-- Sehr häufig
											2|[local::at0017], 	-- Häufig
											3|[local::at0018], 	-- Teils/Teils
											4|[local::at0019], 	-- Weniger häufig
											5|[local::at0020]  	-- Gar nicht
										}
									}
								}
							}
						}
					}
				}
			}
		}
		content cardinality matches {0..*; unordered} matches {
			allow_archetype EVALUATION[at0006] occurrences matches {0..*} matches {	-- Fragebogen Auswertung
				include
					archetype_id/value matches {/.*/}
			}
		}
	}


