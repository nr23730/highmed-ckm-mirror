									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0174] occurrences matches {0..*} matches {	-- Begründung der Dosis
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0177] occurrences matches {0..*} matches {	-- Strukturierte Dosis- und Zeitangaben
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.therapeutic_direction(-[a-zA-Z0-9_]+)*\.v1/}
							}
							ELEMENT[at0018] occurrences matches {0..*} matches {	-- Klinische Indikation
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0148] occurrences matches {0..*} matches {	-- Therapiezweck
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0179] occurrences matches {0..1} matches {	-- Klinische Leitlinie
								value matches {
									DV_TEXT matches {*}
								}
							}
							CLUSTER[at0113] occurrences matches {0..1} matches {	-- Detaillierte Angaben zu der Verordnung
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0012] occurrences matches {0..1} matches {	-- Beginn der Verordnung
										value matches {
											DV_DATE_TIME matches {*}
										}
									}
									ELEMENT[at0013] occurrences matches {0..1} matches {	-- Ende der Verordnung
										value matches {
											DV_DATE_TIME matches {*}
										}
									}
									ELEMENT[at0011] occurrences matches {0..*} matches {	-- Startkriterium für die Verordnung
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0016] occurrences matches {0..*} matches {	-- Stopkriterium für die Verordnung
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0060] occurrences matches {0..1} matches {	-- Anwendungen durchgeführt
										value matches {
											DV_COUNT matches {
