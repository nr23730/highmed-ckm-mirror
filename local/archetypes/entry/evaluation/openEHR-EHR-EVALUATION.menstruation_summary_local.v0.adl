definition
	EVALUATION[at0000] matches {    -- Menstruation summary
		data matches {
			ITEM_TREE[at0001] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0025] occurrences matches {0..1} matches {    -- Overall description
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {    -- Menstrual status
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0026,    -- Premenarchal
									at0005,    -- Premenopausal
									at0006,    -- Perimenopausal
									at0007]    -- Postmenopausal
								}
							}
						}
					}
					ELEMENT[at0002] occurrences matches {0..1} matches {    -- Age at menarche
						value matches {
							DV_DURATION matches {
								value matches {PYM/|>=P0M|}
							}
						}
					}
					CLUSTER[at0010] occurrences matches {0..*} matches {    -- Per episode
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0011] occurrences matches {0..1} matches {    -- Episode label
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0012] occurrences matches {0..1} matches {    -- Episode start date
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0013] occurrences matches {0..1} matches {    -- Description
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0014] occurrences matches {0..1} matches {    -- Typical pattern
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0015,    -- Regular
											at0016]    -- Irregular
										}
									}
								}
							}
							ELEMENT[at0017] occurrences matches {0..1} matches {    -- Typical cycle length
								value matches {
									DV_DURATION matches {
										value matches {PD/|P1D..P999D|}
									}
									DV_INTERVAL<DV_DURATION> matches {
										lower matches {
											DV_DURATION matches {
												value matches {PD/|P1D..P999D|}
											}
										}
										upper matches {
											DV_DURATION matches {
												value matches {PD/|P1D..P999D|}
											}
										}
									}
								}
							}
							ELEMENT[at0018] occurrences matches {0..1} matches {    -- Typical menses duration
								value matches {
									DV_DURATION matches {
										value matches {PD/|P1D..P999D|}
									}
									DV_INTERVAL<DV_DURATION> matches {
										lower matches {
											DV_DURATION matches {
												value matches {PD/|P1D..P999D|}
											}
										}
										upper matches {
											DV_DURATION matches {
												value matches {PD/|P1D..P999D|}
											}
										}
									}
								}
							}
							allow_archetype CLUSTER[at0020] occurrences matches {0..*} matches {    -- Additional details
								include
									archetype_id/value matches {/.*/}
							}
							ELEMENT[at0021] occurrences matches {0..1} matches {    -- Episode end date
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0022] occurrences matches {0..1} matches {    -- Episode comment
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {    -- Age at menopause
						value matches {
							DV_DURATION matches {
								value matches {PYM/|>=P0M|}
							}
						}
					}
					ELEMENT[at0023] occurrences matches {0..1} matches {    -- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0008] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0009] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0024] occurrences matches {0..1} matches {    -- Date last updated
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
				}
			}
		}
	}



ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Menstruation summary">
					description = <"Summary or persistent information about an individual's menstruation history.">
				>
				["at0001"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Age at menarche">
					description = <"Age at onset of first menstrual cycle.">
				>
				["at0003"] = <
					text = <"Menstrual status">
					description = <"Statement about the current menstrual activity.">
					comment = <"If this data element is updated, the 'Date last updated' in Protocol should also be amended.">
				>
				["at0004"] = <
					text = <"Age at menopause">
					description = <"Age at cessation of menstrual cycles.">
				>
				["at0005"] = <
					text = <"Premenopausal">
					description = <"After menarche, preceding menopause and without symptoms related to the hormonal changes preceding menopause.">
				>
				["at0006"] = <
					text = <"Perimenopausal">
					description = <"Preceding menopause with symptoms related to the hormonal changes leading up to menopause.">
				>
				["at0007"] = <
					text = <"Postmenopausal">
					description = <"After menopause.">
				>
				["at0008"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0009"] = <
					text = <"Extension">
					description = <"Additional information required to capture local content or to align with other reference models/formalisms.">
					comment = <"For example: local information requirements or additional metadata to align with FHIR or CIMI equivalents.">
				>
				["at0010"] = <
					text = <"Per episode">
					description = <"Details about a typical pattern of menstruation during a specified interval of time.">
					comment = <"If the pattern of menstruation changes significantly, or other factors significantly interrupt (for example, a full term pregnancy) or influence (for example, changing contraception methods) the menstrual cycle, then it may be useful to record as a new episode .">
				>
				["at0011"] = <
					text = <"Episode label">
					description = <"A name or label associated with this episode of menstruation.">
					comment = <"The label will allow a specific episode to be distinguished from other episodes with similar or different menstruation patterns. For example: 'Post Pregnancy #2'; 'Commenced POP'.">
				>
				["at0012"] = <
					text = <"Episode start date">
					description = <"Date when this episode commenced.">
					comment = <"Can be a partial date, for example, only a year.">
				>
				["at0013"] = <
					text = <"Description">
					description = <"Narrative description about this episode.">
				>
				["at0014"] = <
					text = <"Typical pattern">
					description = <"The usual predictability of menstruation during this episode.">
				>
				["at0015"] = <
					text = <"Regular">
					description = <"The start of menses is roughly predictable from month to month.">
				>
				["at0016"] = <
					text = <"Irregular">
					description = <"The start of menses is not predictable month to month.">
				>
				["at0017"] = <
					text = <"Typical cycle length">
					description = <"The usual length of cycle during this episode.">
				>
				["at0018"] = <
					text = <"Typical menses duration">
					description = <"The usual duration of menses during this episode.">
				>
				["at0020"] = <
					text = <"Additional details">
					description = <"Additional structured details about this episode of menstruation.">
				>
				["at0021"] = <
					text = <"Episode end date">
					description = <"Date when this episode ceased.">
					comment = <"Can be a partial date, for example, only a year. This data field will be empty if the episode is current and ongoing.">
				>
				["at0022"] = <
					text = <"Episode comment">
					description = <"Additional narrative about this episode of menstruation not captured in other fields.">
				>
				["at0023"] = <
					text = <"Comment">
					description = <"Additional narrative about the overall patterns and episodes of menstruation not captured in other fields.">
				>
				["at0024"] = <
					text = <"Date last updated">
					description = <"Date when the Menstruation summary, including episodes, was updated.">
				>
				["at0025"] = <
					text = <"Overall description">
					description = <"Narrative description about the overall pattern of menstruation.">
				>
				["at0026"] = <
					text = <"Premenarchal">
					description = <"Before the first onset of menstruation.">
				>
			>
		>
		["nb"] = <
			items = <
				["at0000"] = <
					text = <"Sammendrag om menstruasjon">
					description = <"Sammendrag eller varig (persistent) informasjon om et individs menstruasjonshistorikk.">
				>
				["at0001"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Alder ved menarke">
					description = <"Alder ved første menstruasjon.">
				>
				["at0003"] = <
					text = <"Menstruasjonsstatus">
					description = <"Status relatert til stadium under reproduktiv periode.">
					comment = <"Hvis dette dataelementet er oppdatert, bør \"Dato sist oppdatert\" i Protocol også endres.">
				>
				["at0004"] = <
					text = <"Alder ved menopause">
					description = <"Alderen menopausen startet.">
				>
				["at0005"] = <
					text = <"Premenopausal">
					description = <"Etter menarké, forut for menopausen og uten symptomer relatert til hormonelle endringer i klimakteriet.">
				>
				["at0006"] = <
					text = <"Perimenopausal">
					description = <"Forut for menopausen, med symptomer relatert til hormonelle endringer i klimakteriet.">
				>
				["at0007"] = <
					text = <"Postmenopausal">
					description = <"Etter menopausen.">
				>
				["at0008"] = <
					text = <"*Item tree (en)">
					description = <"@ internal @">
				>
				["at0009"] = <
					text = <"Tilleggsinformasjon">
					description = <"Ytterligere informasjon som trengs for å kunne registrere lokalt definert innhold eller for å tilpasse til andre referansemodeller/formalismer.">
					comment = <"For eksempel lokale informasjonsbehov eller ytterligere metadata for å kunne tilpasse til tilsvarende konsepter i FHIR.">
				>
				["at0010"] = <
					text = <"Per tidsepisode">
					description = <"Detaljer om et typisk menstruasjonsmønster i et angitt tidsintervall.">
					comment = <"Hvis menstruasjonsmønsteret endres betydelig, eller andre faktorer avbryter (for eksempel et fullgått svangerskap) eller påvirker menstruasjonssyklusen (for eksempel endring av prevensjonsmetoder), kan det være nyttig å registrere det inn som en ny episode.">
				>
				["at0011"] = <
					text = <"Episodenavn">
					description = <"Et navn eller en betegnelse for denne episoden i menstruasjonshistorikken.">
					comment = <"Navnet gjør det mulig å skille en bestemt episode fra andre episoder med lignende eller forskjellige menstruasjonsmønstre. For eksempel: \"Etter andre svangerskap\", \"Påbegynt p-pille\".">
				>
				["at0012"] = <
					text = <"Episodens startdato">
					description = <"Dato da episoden startet.">
					comment = <"Kan være en deldato, for eksempel kun årstall.">
				>
				["at0013"] = <
					text = <"Beskrivelse">
					description = <"Fritekstbeskrivelse om denne episoden.">
				>
				["at0014"] = <
					text = <"Typisk mønster">
					description = <"Regulariteten av menstruasjonen i løpet av denne episoden.">
				>
				["at0015"] = <
					text = <"Regelmessig">
					description = <"Starten av menstruasjonen er grovt sett forutsigbar fra måned til måned.">
				>
				["at0016"] = <
					text = <"Uregelmessig">
					description = <"Starten av menstruasjonen er uforutsigbar fra måned til måned.">
				>
				["at0017"] = <
					text = <"Typisk sykluslengde">
					description = <"Vanlig sykluslengde i løpet av denne episoden.">
				>
				["at0018"] = <
					text = <"Typisk varighet">
					description = <"Den vanlige varigheten av menstruasjon i løpet av denne episoden.">
				>
				["at0020"] = <
					text = <"Ytterligere detaljer">
					description = <"Ytterligere strukturerte detaljer om denne episoden.">
				>
				["at0021"] = <
					text = <"Episodens sluttdato">
					description = <"Datoen episoden opphørte.">
					comment = <"Kan være en deldato, for eksempel årstall. Dette dataelementet vil være tomt om episoden er pågående.">
				>
				["at0022"] = <
					text = <"Kommentar til episoden">
					description = <"Ytterligere fritekst om menstruasjonen innenfor den spesifiserte episoden, som ikke er omfattet av andre felt.">
				>
				["at0023"] = <
					text = <"Kommentar">
					description = <"Ytterligere fritekst om menstruasjonsmønster og epsioder som ikke er omfattet av andre felt.">
				>
				["at0024"] = <
					text = <"Sist oppdatert">
