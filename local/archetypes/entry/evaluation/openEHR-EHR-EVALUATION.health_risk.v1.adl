				["at0000"] = <
					text = <"Εκτίμηση κινδύνου για την υγεία">
					description = <"Αξιολόγηση της πιθανότητας μελλοντικών δυσμενών επιπτώσεων στην υγεία, όπως καθορίζεται από αναγνωρισμένους παράγοντες κινδύνου.">
				>
				["at0001"] = <
					text = <"structure">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Υγειονομικό ρίσκο">
					description = <"Προσδιορισμός της πιθανής μελλοντικής ασθένειας, κατάστασης ή προβλήματος υγείας για το οποίο αξιολογείται ο κίνδυνος, ονομαστικά.">
					comment = <"Προτιμάται η κωδικοποίηση του «κινδύνου για την υγεία» με ορολογία, όπου είναι δυνατόν. Το ελεύθερο κείμενο θα πρέπει να χρησιμοποιείται μόνο εάν δεν υπάρχει κατάλληλη διαθέσιμη ορολογία. Για παράδειγμα: κίνδυνος καρδιαγγειακής νόσου, με παράγοντες κινδύνου υπέρτασης και υπερχοληστερολαιμίας.">
				>
				["at0003"] = <
					text = <"Εκτίμηση κινδύνου">
					description = <"Αξιολόγηση του κινδύνου για την υγεία.">
					comment = <"Μπορεί να υπάρχουν πολλαπλές παραλλαγές στην εκτίμηση του κινδύνου. Ο τύπος δεδομένων Choice επιτρέπει την καταγραφή της αξιολόγησης είτε ως ελεύθερο κείμενο είτε ως σύνολα τιμών (όπως χαμηλή, μεσαία ή hig). Ο τύπος δεδομένων αναλογίας επιτρέπει την καταγραφή ενός ποσοστού, ενός λόγου ή ενός κλάσματος. Ο τύπος δεδομένων ποσότητας επιτρέπει την καταγραφή ενός δεκαδικού αριθμού.">
				>
				["at0004"] = <
					text = <"Αιτιολόγηση">
					description = <"Αιτιολόγηση αυτής της εκτίμησης κινδύνου.">
					comment = <"Οι λεπτομέρειες που μπορούν να προστεθούν σε αυτό το στοιχείο δεδομένων μπορεί να περιλαμβάνουν πληροφορίες σχετικά με τον πληθυσμό, τις υποομάδες κ. λπ. βάσει των οποίων αξιολογείται ο προσδιορισμός του κινδύνου.">
				>
				["at0010"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0011"] = <
					text = <"*Extension(en)">
					description = <"*Additional information required to capture local content or to align with other reference models/formalisms.(en)">
				>
				["at0012"] = <
					text = <"Σύνδεσμος σε αποδεικτικά στοιχεία">
					description = <"Προσδιορισμός της διαδρομής προς το αρχέτυπο ή τον κόμβο δεδομένων για την απόδειξη κινδύνου.">
				>
				["at0013"] = <
					text = <"Παράγοντας κινδύνου">
					description = <"Προσδιορισμός του παράγοντα κινδύνου, ονομαστικά.">
					comment = <"Για παράδειγμα: υπέρταση και υπερχοληστερολαιμία, τα οποία μπορούν να χρησιμοποιηθούν ως μέρος της συνολικής αξιολόγησης για καρδιαγγειακές παθήσεις. ή έναν γενετικό δείκτη. Κωδικοποίηση
«Παράγοντας κινδύνου» με ορολογία, όπου είναι δυνατόν.">
				>
				["at0014"] = <
					text = <"Περιγραφή">
					description = <"Αφηγηματική περιγραφή σχετικά με τον παράγοντα κινδύνου.">
				>
				["at0015"] = <
					text = <"Σχόλιο">
					description = <"Πρόσθετη αφήγηση σχετικά με την εκτίμηση κινδύνου που δεν αποτυπώνεται σε άλλους τομείς.">
				>
				["at0016"] = <
					text = <"Παράγοντες κινδύνου">
					description = <"Λεπτομέρειες σχετικά με κάθε πιθανό παράγοντα κινδύνου.">
				>
				["at0017"] = <
					text = <"Παρουσία">
					description = <"Παρουσία του παράγοντα κινδύνου.">
				>
				["at0018"] = <
					text = <"Παρών">
					description = <"Ο παράγοντας κινδύνου έχει εντοπιστεί για αυτό το άτομο.">
				>
				["at0019"] = <
					text = <"Απών">
					description = <"Ο παράγοντας κινδύνου δεν έχει προσδιοριστεί για αυτό το άτομο.">
