				>
				["at0001"] = <
					text = <"*Tree(en)">
					description = <"*@ internal @(en)">
				>
				["at0002"] = <
					text = <"Status der körperlichen Aktivität (PAL)">
					description = <"Kategorie der typischen oder üblichen körperlichen Aktivität einer Person.">
					comment = <"Wenn möglich, ist eine Codierung mit einer Terminologie wünschenswert.">
				>
				["at0003"] = <
					text = <"Beschreibung">
					description = <"Eine Beschreibung der typischen körperlichen Aktivität des Individuums.">
				>
				["at0005"] = <
					text = <"Barriere">
					description = <"Identifizierter Faktor, der verhindert, dass das Individuum körperlich aktiver wird.">
				>
				["at0006"] = <
					text = <"Unterstützung">
					description = <"Identifizierter Faktor, der das Individuum dabei unterstützen kann, körperlich aktiver zu werden.">
					comment = <"Zum Beispiel: lebt in der Nähe eines Schwimmbades; oder vorher ein Spitzensportler gewesen.">
