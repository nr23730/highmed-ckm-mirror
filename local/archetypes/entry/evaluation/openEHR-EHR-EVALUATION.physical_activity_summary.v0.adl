					text = <"Typische kräftige Übung">
					description = <"Typische Anzahl von Minuten intensiven Trainings.">
				>
				["at0008"] = <
					text = <"Typische moderate Übung">
					description = <"Typische Anzahl von Minuten mäßiger Bewegung.">
				>
				["at0009"] = <
					text = <"Extrem inaktiv">
					description = <"Die Person ist extrem inaktiv, zum Beispiel ein bettlägeriger Patient.">
				>
				["at0010"] = <
					text = <"Bewegungsmangel">
					description = <"Die Person verbringt die meiste Zeit im Sitzen, zum Beispiel ein Büroangestellter, der wenig oder gar keine Bewegung bekommt.">
				>
				["at0011"] = <
					text = <"Mäßig aktiv">
					description = <"Die Person ist mäßig aktiv, zum Beispiel ein Bauarbeiter oder eine Person, die täglich eine Stunde läuft.">
				>
				["at0012"] = <
					text = <"Kräftig aktiv">
					description = <"Die Person ist sehr aktiv, zum Beispiel ein Arbeiter oder eine Person, die zwei Stunden täglich schwimmt.">
				>
				["at0013"] = <
					text = <"Extrem aktiv">
					description = <"Der Einzelne ist äußerst aktiv, zum Beispiel ein konkurrenzfähiger Radfahrer.">
				>
				["at0014"] = <
					text = <"*Tree(en)">
					description = <"*@ internal @(en)">
				>
				["at0015"] = <
					text = <"Erweiterung">
					description = <"Zusätzliche Informationen zur Erfassung lokaler Inhalte oder Anpassung an andere Referenzmodelle/Formalismen.">
					comment = <"Beispiel: Anforderungen an lokale Informationen oder zusätzliche Metadaten zur Ausrichtung auf FHIR- oder CIMI-Entsprechungen.">
				>
				["at0016"] = <
					text = <"Zuletzt aktualisiert">
					description = <"Das Datum, an dem die Zusammenfassung der körperlichen Aktivität zuletzt aktualisiert wurde.">
				>
			>
		>
	>
