definition
	EVALUATION[at0000] matches {    -- Medication Plan
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0032] occurrences matches {0..1} matches {    -- Name of Medication
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[ac0000]
								}
							}
						}
					}
					CLUSTER[at0021] occurrences matches {0..1} matches {    -- Dosage (in mg)
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0022] occurrences matches {0..1} matches {    -- Mornings
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::124]>
										list = <
											["1"] = <
												units = <"mg">
											>
										>
									>
								}
							}
							ELEMENT[at0023] occurrences matches {0..1} matches {    -- Midday
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::124]>
										list = <
											["1"] = <
												units = <"mg">
											>
										>
									>
								}
							}
							ELEMENT[at0024] occurrences matches {0..1} matches {    -- Afternoon
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::124]>
										list = <
											["1"] = <
												units = <"mg">
											>
										>
									>
								}
							}
							ELEMENT[at0025] occurrences matches {0..1} matches {    -- Nights
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::124]>
										list = <
											["1"] = <
												units = <"mg">
											>
										>
									>
								}
							}
						}
					}
					ELEMENT[at0026] occurrences matches {0..*} matches {    -- Reason for taking
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0017] occurrences matches {0..1} matches {    -- Taking since
						value matches {
							DV_DATE matches {*}
						}
					}
					ELEMENT[at0019] occurrences matches {0..*} matches {    -- Notes
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0018] occurrences matches {0..1} matches {    -- Discontinued on
						value matches {
							DV_DATE matches {*}
						}
					}
					ELEMENT[at0016] occurrences matches {0..*} matches {    -- Self-medication
						value matches {
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0033] occurrences matches {0..1} matches {    -- Perscribed from
						items cardinality matches {1..*; unordered} matches {
							CLUSTER[at0034] occurrences matches {0..1} matches {    -- Structured name of a person
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0035] occurrences matches {0..*} matches {    -- Title
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0036] occurrences matches {0..*} matches {    -- Given name
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0037] occurrences matches {0..*} matches {    -- Family name
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0038] occurrences matches {0..*} matches {    -- Suffix
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
							CLUSTER[at0039] occurrences matches {0..1} matches {    -- Institution
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0040] occurrences matches {0..1} matches {    -- Name
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0041] occurrences matches {0..1} matches {    -- Contact
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0011] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0030] occurrences matches {0..1} matches {    -- Info Text Block
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}


