				>
				["at0014"] = <
					text = <"Datum des Auförens mit dem Rauchen">
					description = <"Datum, an dem die Person das letzte Mal die angegebene Tabaksorte geraucht hat.">
					comment = <"Kann ein Teildatum sein, z.B. nur ein Jahr. Die Definitionen für ein 'Datum des Aufhörens mit dem Rauchen' sind sehr unterschiedlich und können über das Datenelement 'Aufhören mit dem Rauchen' im Abschnitt 'Protokoll' dieses Archetyps definiert werden. Dieses Datum ist identisch mit dem 'Enddatum der Episode' der letzten Episode. Dieses Datum könnte von der Entscheidungshilfe genutzt werden, um festzustellen, ob die Person, zum Beispiel in den ersten 12 Monaten nach der Kündigung, einem Rückfallrisiko ausgesetzt ist.

">
				>
				["at0015"] = <
					text = <"Rauchbeginn">
					description = <"Das Datum oder Teildatum, an dem die Person zum ersten Mal mit dem Rauchen von Tabak jeglicher Art begonnen hat.">
					comment = <"Kann ein Teildatum sein, z.B. nur ein Jahr. Dieses Datum könnte z.B. bedeuten, wann die Person jeden Freitagabend oder auf Partys mit dem Rauchen begonnen hat.">
				>
				["at0016"] = <
					text = <"Zeitpunkt des Aufhörens">
					description = <"Das Datum, an dem die Person zuletzt aufgehört hat, Tabak jeglicher Art zu konsumieren.">
					comment = <"Kann ein Teiltermin sein, z.B. nur ein Jahr. Dieses Datum könnte von der Entscheidungshilfe genutzt werden, um festzustellen, ob die Person, zum Beispiel in den ersten 12 Monaten nach dem Aufhören, einem Rückfallrisiko ausgesetzt ist.">
				>
				["at0017"] = <
					text = <"Anzahl Packungen pro Jahr">
					description = <"Schätzung der kumulierten Menge an Tabak, die geraucht wurde.">
					comment = <"Wird häufig bei der Beurteilung des Zigarettenkonsums verwendet. Sie errechnet sich aus der Anzahl der Zigarettenpackungen, die pro Tag geraucht werden, multipliziert mit der Anzahl der Jahre, die die Person geraucht hat. Ein Packungsjahr entspricht 365 Packungen Zigaretten. Details zur Berechnung der Packungsjahre für andere Tabaksorten finden Sie unter http://smokingpackyears.com. Die Definition einer Packung kann im Protokoll dieses Archetyps mit dem Datenelement 'Definition Packung' beschrieben werden.
">
				>
				["at0019"] = <
					text = <"Zusätzliche Anmerkungen">
					description = <"Zusätzliche Anmerkungen über das Tabak rauchen, die in anderen Bereichen nicht erfasst wurden.">
					comment = <"Zum Beispiel: Raucherentwöhnung oder reduzierte Menge bei Schwangerschaft.">
				>
				["at0021"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
