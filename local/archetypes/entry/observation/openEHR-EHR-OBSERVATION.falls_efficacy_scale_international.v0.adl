	details = <
		["de"] = <
			language = <[ISO_639-1::de]>
			purpose = <"Wird verwendet um die sturzassoziierte Selbstwirksamkeit bei Senioren mittels Fragebogen zu ermitteln.">
			use = <"Dieser Fragebogen wird verwendet, um eine aussagekräftige Bewertung zum Sturzrisiko des Senioren/ der Seniorin zu erhalten. Zur Erfassung wird der standardisierte Fragebogen der internationalen Version des Falls Efficacy Scale verwendet.">
			keywords = <"FES-I", "Falls Effifacy Scale", "Sturz", "geriatrisches Assessement">
			misuse = <"Nicht zur Erfassung des nicht internationalen Falls Efficacy Score.
Nicht zur Erfassung des Short Falls Efficacy Scale geeignet.">
			copyright = <"© HiGHmed">
		>
	>
