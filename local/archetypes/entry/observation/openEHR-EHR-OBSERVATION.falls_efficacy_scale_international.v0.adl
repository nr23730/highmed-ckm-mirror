definition
	OBSERVATION[at0000] matches {	-- Falls Efficacy Scale - International
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- Jedes Ereignis
						data matches {
							ITEM_TREE[at0003] matches {	-- Baum
								items cardinality matches {17; unordered} matches {
									ELEMENT[at0004] matches {	-- Den Hausputz machen
										value matches {
											1|[local::at0005], 	-- Keinerlei Bedenken
											2|[local::at0006], 	-- Einige Bedenken
											3|[local::at0007], 	-- Ziemliche Bedenken
											4|[local::at0009]  	-- Sehr große Bedenken
										}
									}
									ELEMENT[at0010] matches {	-- Sich an- oder ausziehen
										value matches {
											1|[local::at0011], 	-- Keinerlei Bedenken
											2|[local::at0012], 	-- Einige Bedenken
											3|[local::at0013], 	-- Ziemliche Bedenken
											4|[local::at0014]  	-- Sehr große Bedenken
										}
									}
									ELEMENT[at0016] matches {	-- Einfache Mahlzeit zubereiten
										value matches {
											1|[local::at0017], 	-- Keinerlei Bedenken
											2|[local::at0018], 	-- Einige Bedenken
											3|[local::at0019], 	-- Ziemliche Bedenken
											4|[local::at0020]  	-- Sehr große Bedenken
										}
									}
									ELEMENT[at0022] matches {	-- Ein Bad nehmen
										value matches {
											1|[local::at0023], 	-- Keinerlei Bedenken
											2|[local::at0024], 	-- Einige Bedenken
											3|[local::at0025], 	-- Ziemliche Bedenken
											4|[local::at0026]  	-- Sehr große Bedenken
										}
									}
									ELEMENT[at0028] matches {	-- In einem Laden einkaufen
										value matches {
											1|[local::at0029], 	-- Keinerlei Bedenken
											2|[local::at0030], 	-- Einige Bedenken
											3|[local::at0031], 	-- Ziemliche Bedenken
											4|[local::at0032]  	-- Sehr große Bedenken
										}
									}
									ELEMENT[at0034] matches {	-- Von einem Stuhl aufstehen oder hinsetzen
										value matches {
											1|[local::at0035], 	-- Keinerlei Bedenken
											2|[local::at0036], 	-- Einige Bedenken
											3|[local::at0037], 	-- Ziemliche Bedenken
											4|[local::at0038]  	-- Sehr große Bedenken
										}
									}
									ELEMENT[at0040] matches {	-- Eine Treppe hinauf- oder hinuntergehen
										value matches {
											1|[local::at0041], 	-- Keinerlei Bedenken
											2|[local::at0042], 	-- Einige Bedenken
											3|[local::at0043], 	-- Ziemliche Bedenken
											4|[local::at0044]  	-- Sehr große Bedenken
										}
									}
									ELEMENT[at0046] matches {	-- In der Nähe der Wohnung draußen umhergehen
										value matches {
											1|[local::at0047], 	-- Keinerlei Bedenken
											2|[local::at0048], 	-- Einige Bedenken
											3|[local::at0049], 	-- Ziemliche Bedenken
											4|[local::at0050]  	-- Sehr große Bedenken
										}
									}
									ELEMENT[at0052] matches {	-- Gegenstände oberhalb des Kopfes/ auf dem Boden
										value matches {
											1|[local::at0053], 	-- Keinerlei Bedenken
											2|[local::at0054], 	-- Einige Bedenken
											3|[local::at0055], 	-- Ziemliche Bedenken
											4|[local::at0056]  	-- Sehr große Bedenken
										}
									}
									ELEMENT[at0058] matches {	-- Das Telefon erreichen, bevor es aufhört zu klingeln
										value matches {
											1|[local::at0059], 	-- Keinerlei Bedenken
											2|[local::at0060], 	-- Einige Bedenken
											3|[local::at0061], 	-- Ziemliche Bedenken
											4|[local::at0062]  	-- Sehr große Bedenken
										}
									}
									ELEMENT[at0064] matches {	-- Auf einer rutschigen Oberfläche gehen
										value matches {
											1|[local::at0065], 	-- Keinerlei Bedenken
											2|[local::at0066], 	-- Einige Bedenken
											3|[local::at0067], 	-- Ziemliche Bedenken
											4|[local::at0068]  	-- Sehr große Bedenken
										}
									}
									ELEMENT[at0070] matches {	-- Einen Freund oder Verwandten besuchen
										value matches {
											1|[local::at0071], 	-- Keinerlei Bedenken
											2|[local::at0072], 	-- Einige Bedenken
											3|[local::at0073], 	-- Ziemliche Bedenken
											4|[local::at0074]  	-- Sehr große Bedenken
										}
									}
									ELEMENT[at0076] matches {	-- In einer Menschenmenge umhergehen
										value matches {
											1|[local::at0077], 	-- Keinerlei Bedenken
											2|[local::at0078], 	-- Einige Bedenken
											3|[local::at0079], 	-- Ziemliche Bedenken
											4|[local::at0080]  	-- Sehr große Bedenken
										}
									}
									ELEMENT[at0082] matches {	-- Auf unebenem Boden gehen
										value matches {
											1|[local::at0083], 	-- Keinerlei Bedenken
											2|[local::at0084], 	-- Einige Bedenken
											3|[local::at0085], 	-- Ziemliche Bedenken
											4|[local::at0086]  	-- Sehr große Bedenken
										}
									}
									ELEMENT[at0088] matches {	-- Eine Steigung hinauf- oder hinuntergehen
										value matches {
											1|[local::at0089], 	-- Keinerlei Bedenken
											2|[local::at0090], 	-- Einige Bedenken
											3|[local::at0091], 	-- Ziemliche Bedenken
											4|[local::at0092]  	-- Sehr große Bedenken
										}
									}
									ELEMENT[at0094] matches {	-- Eine Veranstaltung besuchen
										value matches {
											1|[local::at0095], 	-- Keinerlei Bedenken
											2|[local::at0096], 	-- Einige Bedenken
											3|[local::at0097], 	-- Ziemliche Bedenken
											4|[local::at0098]  	-- Sehr große Bedenken
										}
									}
									ELEMENT[at0101] matches {	-- Gesamtpunktzahl
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..64|}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}


