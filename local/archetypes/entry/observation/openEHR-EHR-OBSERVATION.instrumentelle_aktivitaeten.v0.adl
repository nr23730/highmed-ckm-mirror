		["revision"] = <"0.0.1-alpha">
	>

definition
	OBSERVATION[at0000] matches {	-- Instrumentelle Aktivitäten
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- Jedes Ereignis
						data matches {
							ITEM_TREE[at0003] matches {	-- Baum
								items cardinality matches {6..*; unordered} matches {
									CLUSTER[at0090] matches {	-- Telefon
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0091] occurrences matches {0..1} matches {	-- Benutzt Telefon aus eigener Initiative, wählt Nummern
												value matches {1|[local::at0095]}		-- Ja
											}
											ELEMENT[at0094] occurrences matches {0..1} matches {	-- Wählt einige bekannte Nummern
												value matches {1|[local::at0096]}		-- Ja
											}
											ELEMENT[at0093] occurrences matches {0..1} matches {	-- Nimmt ab, wählt nicht selbstständig
												value matches {1|[local::at0097]}		-- Ja
											}
											ELEMENT[at0092] occurrences matches {0..1} matches {	-- Benutzt das Telefon überhaupt nicht
												value matches {0|[local::at0098]}		-- Ja
											}
										}
									}
									CLUSTER[at0052] matches {	-- Einkaufen
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0053] occurrences matches {0..1} matches {	-- Kauft selbstständig die meisten benötigten Sachen ein
												value matches {1|[local::at0057]}		-- Ja
											}
											ELEMENT[at0056] occurrences matches {0..1} matches {	-- Tätigt wenige Einkäufe
												value matches {0|[local::at0058]}		-- Ja
											}
											ELEMENT[at0054] occurrences matches {0..1} matches {	-- Benötigt bei jedem Einkauf Begleitung
												value matches {0|[local::at0059]}		-- Ja
											}
											ELEMENT[at0055] occurrences matches {0..1} matches {	-- Unfähig zum Einkaufen
												value matches {0|[local::at0060]}		-- Ja
											}
										}
									}
									CLUSTER[at0061] occurrences matches {0..1} matches {	-- Kochen
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0062] occurrences matches {0..1} matches {	-- Plant und kocht erforderliche Mahlzeit selbstständig
												value matches {1|[local::at0066]}		-- Ja
												null_flavour existence matches {0..1} matches {
													DV_CODED_TEXT matches {
														defining_code matches {[openehr::273]}
													}
												}
											}
											ELEMENT[at0063] occurrences matches {0..1} matches {	-- Kocht erforderliche Mahlzeit nur nach Vorbereitung durch Drittperson
												value matches {0|[local::at0067]}		-- Ja
												null_flavour existence matches {0..1} matches {
													DV_CODED_TEXT matches {
														defining_code matches {[openehr::273]}
													}
												}
											}
											ELEMENT[at0065] occurrences matches {0..1} matches {	-- Kocht selbstständig, hält aber benötigte Diät nicht ein
												value matches {0|[local::at0068]}		-- Ja
												null_flavour existence matches {0..1} matches {
													DV_CODED_TEXT matches {
														defining_code matches {[openehr::273]}
													}
												}
											}
											ELEMENT[at0064] occurrences matches {0..1} matches {	-- Benötigt vorbereitete und servierte Mahlzeiten
												value matches {0|[local::at0069]}		-- Ja
												null_flavour existence matches {0..1} matches {
													DV_CODED_TEXT matches {
														defining_code matches {[openehr::273]}
													}
												}
											}
										}
									}
									CLUSTER[at0070] occurrences matches {0..1} matches {	-- Haushalt
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0075] occurrences matches {0..1} matches {	-- Hält Haushalt instand oder benötigt zeitweise Hilfe bei schweren Arbeiten
												value matches {1|[local::at0099]}		-- Ja
												null_flavour existence matches {0..1} matches {
													DV_CODED_TEXT matches {
														defining_code matches {[openehr::273]}
													}
												}
											}
											ELEMENT[at0077] occurrences matches {0..1} matches {	-- Führt selbstständig kleine Hausarbeiten aus
												value matches {1|[local::at0100]}		-- Ja
												null_flavour existence matches {0..1} matches {
													DV_CODED_TEXT matches {
														defining_code matches {[openehr::273]}
													}
												}
											}
											ELEMENT[at0078] occurrences matches {0..1} matches {	-- Benötigt Hilfe in allen Haushaltsverrichtungen
												value matches {1|[local::at0101]}		-- Ja
												null_flavour existence matches {0..1} matches {
													DV_CODED_TEXT matches {
														defining_code matches {[openehr::273]}
													}
												}
											}
											ELEMENT[at0076] occurrences matches {0..1} matches {	-- Nimmt überhaupt nicht teil an täglichen Verrichtungen im Haushalt
												value matches {0|[local::at0102]}		-- Ja
												null_flavour existence matches {0..1} matches {
													DV_CODED_TEXT matches {
														defining_code matches {[openehr::273]}
													}
												}
											}
										}
									}
									CLUSTER[at0071] occurrences matches {0..1} matches {	-- Wäsche
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0079] occurrences matches {0..1} matches {	-- Wäscht sämtliche eigene Wäsche
												value matches {1|[local::at0103]}		-- Ja
												null_flavour existence matches {0..1} matches {
													DV_CODED_TEXT matches {
														defining_code matches {[openehr::273]}
													}
												}
											}
											ELEMENT[at0081] occurrences matches {0..1} matches {	-- Wäscht kleine Sachen
												value matches {1|[local::at0104]}		-- Ja
												null_flavour existence matches {0..1} matches {
													DV_CODED_TEXT matches {
														defining_code matches {[openehr::273]}
													}
												}
											}
											ELEMENT[at0080] occurrences matches {0..1} matches {	-- Gesamte wäsche muss auswärts gewaschen werden
												value matches {0|[local::at0105]}		-- Ja
												null_flavour existence matches {0..1} matches {
													DV_CODED_TEXT matches {
														defining_code matches {[openehr::273]}
													}
												}
											}
										}
									}
									CLUSTER[at0072] matches {	-- Transportmittel
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0082] occurrences matches {0..1} matches {	-- Benutzt unäbhängig öffentliche Transportmittel, eigenes Auto
												value matches {1|[local::at0106]}		-- Ja
											}
											ELEMENT[at0086] occurrences matches {0..1} matches {	-- Bestellt und benutzt selbstständig Taxi, jedoch keine öffentlichen Verkehrsmittel
												value matches {1|[local::at0107]}		-- Ja
											}
											ELEMENT[at0085] occurrences matches {0..1} matches {	-- Benutzt öffentliche Transportmittel in Begleitung
												value matches {1|[local::at0108]}		-- Ja
											}
											ELEMENT[at0083] occurrences matches {0..1} matches {	-- Beschränkte Fahrten im Taxi oder Auto in Begleitung
												value matches {0|[local::at0109]}		-- Ja
											}
											ELEMENT[at0084] occurrences matches {0..1} matches {	-- Reist überhaupt nicht
												value matches {0|[local::at0110]}		-- Ja
											}
										}
									}
									CLUSTER[at0073] matches {	-- Medikamente
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0087] occurrences matches {0..1} matches {	-- Nimmt Medikamente in genauer Dosierung und zum korrekten Zeitpunkt eigenverantwortlich
												value matches {1|[local::at0114]}		-- Ja
											}
											ELEMENT[at0089] occurrences matches {0..1} matches {	-- Nimmt vorbereitete Medikamente korrekt
												value matches {0|[local::at0115]}		-- Ja
											}
											ELEMENT[at0088] occurrences matches {0..1} matches {	-- Kann korrekte Einnahme von Medikamenten nicht handhaben
												value matches {0|[local::at0116]}		-- Ja
											}
										}
									}
									CLUSTER[at0074] matches {	-- Geldhaushalt
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0111] occurrences matches {0..1} matches {	-- Regelt finanzielle Geschäfte selbstständig (Budget, Schecks, Einzahlung, Gang zur Bank).
												value matches {1|[local::at0117]}		-- Ja
											}
											ELEMENT[at0113] occurrences matches {0..1} matches {	-- Erledigt tägliche kleinere Ausgaben, benötigt aber Hilfe bei Einzahlung, Bankgeschäften
												value matches {1|[local::at0118]}		-- Ja
											}
											ELEMENT[at0112] occurrences matches {0..1} matches {	-- Ist nicht mehr fähig mit Geld umzugehen
												value matches {0|[local::at0119]}		-- Ja
											}
										}
									}
									ELEMENT[at0048] matches {	-- Gesamtpunktzahl
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..8|}
											}
										}
									}
								}
							}
						}
						state matches {
							ITEM_TREE[at0120] matches {	-- Baum
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0126] occurrences matches {0..*} matches {	-- Einflussfaktoren
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0121] matches {	-- Baum
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0122] occurrences matches {0..1} matches {	-- Geschlecht
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0123, 	-- Weiblich
									at0124]	-- Männlich
								}
							}
						}
					}
					allow_archetype CLUSTER[at0125] occurrences matches {0..*} matches {	-- Erweiterung
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


