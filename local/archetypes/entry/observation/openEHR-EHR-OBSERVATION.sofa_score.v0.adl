concept
	[at0000]	-- Sofa score
language
	original_language = <[ISO_639-1::de]>
description
	original_author = <
		["name"] = <"Sarah Ballout">
		["organisation"] = <"MHH-Hannover">
		["email"] = <"ballout.sarah@mh-hannover.de">
		["date"] = <"2020-04-23">
	>
	details = <
		["de"] = <
			language = <[ISO_639-1::de]>
			purpose = <"Zur Repräsenation des SOFA-Score-Wertes, der den Grad der Organfunktionsstörung anzeigt und zur Ermittlung des Mortalitätsrisikos eines Patienten beiträgt. ">
			use = <"Zur Repräsentation des Zustandes eines Patienten mit Sepsis und das Ausmaß der Organschädigung im Verlauf einer Behandlung auf der Intensivstation. 

Zur Bestimmung des SOFA-Scores werden sechs Organe anhand spezifischer Parameter bewertet. Jedes Organsystem wird mit einer Punktzahl zwischen 0 Punkten (normale Funktion) und 4 Punkten (sehr eingeschränkte Funktion links) bewertet. ">
			keywords = <"SOFA-Score", "Organdysfunktion ", "Mortalitätsrisiko ", "Sequential Organ Failure Assessment">
			misuse = <"Nicht zur Repräsentation der tatsächlichen Werte der Atemfrequenz, des Blutdrucks oder der Glasgow-Koma-Skala verwenden. Verwenden Sie für diese Fälle die Archetypen Atmung, Blutdruck oder Glasgow-Koma-Skala.

Nicht zur Repräsentation von Risikopatienten durch Sepsis außerhalb von Intensivstationen verwenden. Verwenden Sie hierfür den Archetypen qSOFA-Score. ">
			copyright = <"© HiGHmed">
		>
	>
