definition
	OBSERVATION[at0000] matches {	-- Sofa score
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					POINT_EVENT[at0002] occurrences matches {0..*} matches {	-- Beliebiges Ereignis zu einem Zeitpunkt
						data matches {
							ITEM_TREE[at0003] matches {	-- Baum
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {	-- Atemtätigkeit
										value matches {
											1|[local::at0016], 	-- < 400 
											2|[local::at0017], 	-- < 300
											3|[local::at0018], 	-- < 200 und Beatmung 
											4|[local::at0019]  	-- < 100 und Beatmung 
										}
									}
									ELEMENT[at0005] occurrences matches {0..1} matches {	-- Zentrales Nervensystem
										value matches {
											1|[local::at0020], 	-- 13-14 
											2|[local::at0021], 	-- 10-12 
											3|[local::at0022], 	-- 6-9 
											4|[local::at0023]  	-- <6 
										}
									}
									ELEMENT[at0006] occurrences matches {0..1} matches {	-- Herz-Kreislauf-System
										value matches {
											1|[local::at0024], 	-- MAP < 70 mmHg 
											2|[local::at0025], 	-- Dopamin ≤ 5 µg/kg/min oder Dobutamin (Dosierung egal) 
											3|[local::at0026], 	-- Dopamin > 5 µg/kg/min oder Adrenalin ≤ 0,1 µg/kg/min oder Noradrenalin ≤ 0,1 µg/kg/min 
											4|[local::at0027]  	-- Dopamin > 15 µg/kg/min oder Adrenalin > 0,1 µg/kg/min oder Noradrenalin > 0,1 µg/kg/min 
										}
									}
									ELEMENT[at0007] occurrences matches {0..1} matches {	-- Leberfunktion
										value matches {
											1|[local::at0028], 	-- 1,2 -1,9 mg/dl 
											2|[local::at0029], 	-- 2,0 - 5,9 mg/dl 
											3|[local::at0030], 	-- 6,0 - 11,9 mg/dl 
											4|[local::at0031]  	-- >12,0 mg/dl 
										}
									}
									ELEMENT[at0008] occurrences matches {0..1} matches {	-- Blutgerinnung
										value matches {
											1|[local::at0032], 	-- < 150 
											2|[local::at0033], 	-- < 100
											3|[local::at0034], 	-- < 50 
											4|[local::at0035]  	-- < 20 
										}
									}
									ELEMENT[at0009] occurrences matches {0..1} matches {	-- Nierenfunktion
										value matches {
											1|[local::at0036], 	-- 1,2 - 1,9 mg/dl 
											2|[local::at0037], 	-- 2,0 - 3,4 mg/dl 
											3|[local::at0038], 	-- 3,5 - 4,9 mg/dl 
											4|[local::at0039]  	-- > 5 mg/dl 
										}
									}
									ELEMENT[at0010] occurrences matches {0..1} matches {	-- SOFA-Score
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..4|}
											}
										}
									}
									ELEMENT[at0011] occurrences matches {0..1} matches {	-- Kommentar
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0040] occurrences matches {0..1} matches {	-- Ist das ein COVID-19 Patient?
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0041, 	-- Positiv bestätigt
													at0042, 	-- Verdachtsfall
													at0043, 	-- Unwahrscheinlich
													at0044]	-- Negativ bestätigt
												}
											}
										}
									}
								}
							}
						}
						state matches {
							ITEM_TREE[at0013] matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0014] matches {	-- Baum
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0015] occurrences matches {0..*} matches {	-- Erweiterung
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


