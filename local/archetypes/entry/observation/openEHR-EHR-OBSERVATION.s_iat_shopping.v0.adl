	openEHR-EHR-OBSERVATION.s_iat_shopping.v0

concept
	[at0000]	-- short-Internet Addiction Test (s-IAT-shopping)
language
	original_language = <[ISO_639-1::de]>
description
	original_author = <
		["name"] = <"Darin Leonhardt">
		["organisation"] = <"Peter L. Reichertz Institut für Medizinische Informatik">
		["email"] = <"leonhardt.darin@mh-hannover.de">
		["date"] = <"2023-02-13">
	>
	details = <
		["de"] = <
			language = <[ISO_639-1::de]>
			purpose = <"Zur Erfassung von exzessiven oder pathologischen Internetnutzung sowie deren Folgen im deutschen als auch internationalen Sprachraum. Es handelt sich hierbei, um eine gekürzte und modifizierte Version des Internet Addicition Tests von Young (1998b). Der short Internet Addiction Test (s-IAT) besteht aus 12 Items und zwei Subskalen: „zeitliche Beeinträchtigung/Kontrollverlust\" und „soziale Belange/Craving\".
">
			use = <"Dient der Erfassung von exzessiver oder pathologischer Internetnutzung sowie deren Folgen im deutschen als auch internationalen Sprachraum.">
			keywords = <"Pathologische Internetnutzung, Internet Addiction Test, Verhaltenssucht, s-IAT, s-IAT-shopping, short-Internet Addiction Test", ...>
			misuse = <"Nicht zur Repräsentation von anderen Fragebögen verwenden.">
			copyright = <"© HiGHmed">
		>
	>
	lifecycle_state = <"in_development">
	other_contributors = <>
	other_details = <
		["licence"] = <"This work is licensed under the Creative Commons Attribution-ShareAlike 3.0 License. To view a copy of this license, visit http://creativecommons.org/licenses/by-sa/3.0/.">
		["custodian_organisation"] = <"HiGHmed">
		["references"] = <"Pawlikowski, M., Altstötter-Gleich, C. und Brand, M. (2013) „Validation and psychometric properties of a short version of Young’s Internet Addiction Test“, Computers in human behavior, 29(3), S. 1212–1223. doi: 10.1016/j.chb.2012.10.014.
Young, K. S. (1998b). Caught in the net: How to recognize the signs of Internet addiction - and a winning strategy for recovery. New York: John Wiley & Sons, Inc.">
		["original_namespace"] = <"org.highmed">
		["original_publisher"] = <"HiGHmed">
		["custodian_namespace"] = <"org.highmed">
