definition
	OBSERVATION[at0000] matches {    -- short-Internet Addiction Test (s-IAT-shopping)
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {    -- Wie oft stellen Sie fest, dass Sie länger als beabsichtigt auf  Shoppingseiten im Internet waren?
										value matches {
											1|[local::at0005],
											2|[local::at0006],
											3|[local::at0007],
											4|[local::at0008],
											5|[local::at0009]
										}
									}
									ELEMENT[at0010] occurrences matches {0..1} matches {    -- Wie oft vernachlässigen Sie alltägliche Pflichten, um mehr  Zeit auf Shoppingseiten im Internet zu verbringen?
										value matches {
											1|[local::at0011],
											2|[local::at0012],
											3|[local::at0013],
											4|[local::at0014],
											5|[local::at0015]
										}
									}
									ELEMENT[at0016] occurrences matches {0..1} matches {    -- Wie häufig leidet Ihre schulische, Ihre universitäre oder Ihre  berufliche Arbeit darunter, dass Sie so viel Zeit auf  Shoppingseiten im Internet verbringen?
										value matches {
											1|[local::at0017],
											2|[local::at0018],
											3|[local::at0019],
											4|[local::at0020],
											5|[local::at0021]
										}
									}
									ELEMENT[at0022] occurrences matches {0..1} matches {    -- Wie häufig reagieren Sie ausweichend oder verteidigend,  wenn Sie jemand fragt, was Sie online tun?
										value matches {
											1|[local::at0024],
											2|[local::at0025],
											3|[local::at0026],
											4|[local::at0027],
											5|[local::at0028]
										}
									}
									ELEMENT[at0023] occurrences matches {0..1} matches {    -- Wie oft reagieren Sie patzig, schimpfen oder sind genervt,  wenn Sie jemand stört, während Sie auf Shoppingseiten im Internet sind?
										value matches {
											1|[local::at0038],
											2|[local::at0039],
											3|[local::at0040],
											4|[local::at0041],
											5|[local::at0042]
										}
									}
									ELEMENT[at0031] occurrences matches {0..1} matches {    -- Wie oft fehlt Ihnen Schlaf, weil Sie spät nachts noch auf  Shoppingseiten im Internet sind?
										value matches {
											1|[local::at0043],
											2|[local::at0044],
											3|[local::at0045],
											4|[local::at0046],
											5|[local::at0047]
										}
									}
									ELEMENT[at0032] occurrences matches {0..1} matches {    -- Wie oft denken Sie an Shoppingseiten im Internet, wenn Sie  offline sind oder stellen sich vor, auf Shoppingseiten im Internet zu sein?
										value matches {
											1|[local::at0048],
											2|[local::at0049],
											3|[local::at0050],
											4|[local::at0051],
											5|[local::at0052]
										}
									}
									ELEMENT[at0033] occurrences matches {0..1} matches {    -- Wie oft ertappen Sie sich dabei zu sagen: „Nur noch ein paar  Minuten“, während Sie auf Shoppingseiten im Internet sind?
										value matches {
											1|[local::at0053],
											2|[local::at0054],
											3|[local::at0055],
											4|[local::at0056],
											5|[local::at0057]
										}
									}
									ELEMENT[at0034] occurrences matches {0..1} matches {    -- Wie häufig versuchen Sie weniger Zeit auf Shoppingseiten im Internet zu verbringen und schaffen es nicht?
										value matches {
											1|[local::at0058],
											2|[local::at0059],
											3|[local::at0060],
											4|[local::at0061],
											5|[local::at0062]
										}
									}
									ELEMENT[at0035] occurrences matches {0..1} matches {    -- Wie häufig versuchen Sie zu verbergen, wie lange Sie auf Shoppingseiten im Internet waren?
										value matches {
											1|[local::at0063],
											2|[local::at0064],
											3|[local::at0065],
											4|[local::at0066],
											5|[local::at0067]
										}
									}
									ELEMENT[at0036] occurrences matches {0..1} matches {    -- Wie oft kommt es vor, dass Sie lieber mehr Zeit auf  Shoppingseiten im Internet verbringen als mit Anderen etwas zu unternehmen?
										value matches {
											1|[local::at0068],
											2|[local::at0069],
											3|[local::at0070],
											4|[local::at0071],
											5|[local::at0072]
										}
									}
									ELEMENT[at0037] occurrences matches {0..1} matches {    -- Wie oft fühlen Sie sich deprimiert, verstimmt oder nervös,  wenn Sie offline sind – was ändert sich, wenn Sie wieder auf Shoppingseiten im Internet sind?
										value matches {
											1|[local::at0073],
											2|[local::at0074],
											3|[local::at0075],
											4|[local::at0076],
											5|[local::at0077]
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0029] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0030] occurrences matches {0..1} matches {    -- Kommentar
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}


