	openEHR-EHR-OBSERVATION.material_values_scale.v0

concept
	[at0000]	-- Material Values Scale (G-MVS)
language
	original_language = <[ISO_639-1::de]>
description
	original_author = <
		["name"] = <"Darin Leonhardt">
		["organisation"] = <"Peter L. Reichertz Institut für Medizinische Informatik">
		["email"] = <"leonhardt.darin@mh-hannover.de">
		["date"] = <"2023-02-07">
	>
	details = <
		["de"] = <
			language = <[ISO_639-1::de]>
			purpose = <"Zur Erfassung der materiellen Werteorientierung von Erwachsenen im deutschen Sprachraum. Die Material Values Scale ist ein Selbsterhebungsinstrument zur Untersuchung von materieller Werteorientierung. Der Fragebogen enthält in der Originalversion die folgenden drei Subskalen: Lebenszufriedenheit, Erfolg und Wohlbefinden. Der Fragebogen besteht aus 15 Items mit einer gestuften Antwortskala von 1 bis 5. Die vorliegende MVS ist die deutsche, validierte Fassung (Müller et al., 2013) der englischsprachigen Originalversion.">
			use = <"Dient der Erfassung der materiellen Werteorientierung im deutschsprachigen Raum. Gemeint sind damit persönliche Überzeugungen bezüglich der zentralen Bedeutung von materiellem Besitztum für Lebenszufriedenheit, Erfolg und Wohlbefinden.">
			keywords = <"Material Values Scale, Materialismus, Kaufzwang, Kaufsucht, Kurzversion, G-MVS", ...>
			misuse = <"Nicht zur Repräsentation von anderen Fragebögen verwenden.">
			copyright = <"© HiGHmed">
		>
	>
	lifecycle_state = <"in_development">
	other_contributors = <>
	other_details = <
		["licence"] = <"This work is licensed under the Creative Commons Attribution-ShareAlike 3.0 License. To view a copy of this license, visit http://creativecommons.org/licenses/by-sa/3.0/.">
		["custodian_organisation"] = <"HiGHmed">
		["references"] = <"Müller, A., Smits, D. J. M., Claes, L., Gefeller, O., Hinz, A., & de Zwaan, M. (2013). The German version of the Material Values Scale. Psycho-Social Medicine, 10, Doc05. doi:10.3205/psm000095
Richins, M. L. (2004). The material values scale: Measurement properties and development of a short form. The journal of consumer research, 31(1), 209–219. doi:10.1086/383436">
		["original_namespace"] = <"org.highmed">
		["original_publisher"] = <"HiGHmed">
