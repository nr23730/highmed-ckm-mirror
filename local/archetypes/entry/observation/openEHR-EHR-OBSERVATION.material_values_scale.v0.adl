definition
	OBSERVATION[at0000] matches {    -- Material Values Scale (G-MVS)
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0005] occurrences matches {0..1} matches {    -- Mein Leben wäre besser, wenn ich bestimmte Dinge besitzen würde, die ich noch nicht habe.
										value matches {
											1|[local::at0008],
											2|[local::at0012],
											3|[local::at0013],
											4|[local::at0014],
											5|[local::at0015]
										}
									}
									ELEMENT[at0006] occurrences matches {0..1} matches {    -- Die Dinge in meinem Besitz sagen eine Menge darüber aus, wie erfolgreich ich bin.
										value matches {
											1|[local::at0010],
											2|[local::at0016],
											3|[local::at0017],
											4|[local::at0018],
											5|[local::at0019]
										}
									}
									ELEMENT[at0020] occurrences matches {0..1} matches {    -- Ich besitze gern Dinge, mit denen ich andere beeindrucken kann.
										value matches {
											1|[local::at0023],
											2|[local::at0024],
											3|[local::at0025],
											4|[local::at0026],
											5|[local::at0027]
										}
									}
									ELEMENT[at0021] occurrences matches {0..1} matches {    -- Ich bewundere Menschen, die teure Häuser, Autos und Kleidung haben.
										value matches {
											1|[local::at0028],
											2|[local::at0029],
											3|[local::at0030],
											4|[local::at0031],
											5|[local::at0032]
										}
									}
									ELEMENT[at0022] occurrences matches {0..1} matches {    -- Ich mag viel Luxus in meinem Leben.
										value matches {
											1|[local::at0033],
											2|[local::at0034],
											3|[local::at0035],
											4|[local::at0036],
											5|[local::at0037]
										}
									}
									ELEMENT[at0038] occurrences matches {0..1} matches {    -- Ich wäre glücklicher, wenn ich mir mehr Dinge kaufen könnte.
										value matches {
											1|[local::at0042],
											2|[local::at0043],
											3|[local::at0044],
											4|[local::at0045],
											5|[local::at0046]
										}
									}
									ELEMENT[at0039] occurrences matches {0..1} matches {    -- Der Erwerb materieller Güter ist einer der wichtigsten Ziele im Leben.
										value matches {
											1|[local::at0047],
											2|[local::at0048],
											3|[local::at0049],
											4|[local::at0050],
											5|[local::at0051]
										}
									}
									ELEMENT[at0040] occurrences matches {0..1} matches {    -- Ich habe eigentlich alles was ich brauche, um das Leben zu genießen.
										value matches {
											1|[local::at0052],
											2|[local::at0053],
											3|[local::at0054],
											4|[local::at0055],
											5|[local::at0056]
										}
									}
									ELEMENT[at0041] occurrences matches {0..1} matches {    -- Wenn ich bessere Dinge hätte, wäre ich auch nicht glücklicher.
										value matches {
											1|[local::at0057],
											2|[local::at0058],
											3|[local::at0059],
											4|[local::at0060],
											5|[local::at0061]
										}
									}
									ELEMENT[at0062] occurrences matches {0..1} matches {    -- Die Dinge, die ich besitze, sind für mich nicht so wichtig.
										value matches {
											1|[local::at0065],
											2|[local::at0066],
											3|[local::at0067],
											4|[local::at0068],
											5|[local::at0069]
										}
									}
									ELEMENT[at0063] occurrences matches {0..1} matches {    -- Auf materielle Dinge lege ich weniger Wert als die meisten Menschen, die ich kenne.
										value matches {
											1|[local::at0070],
											2|[local::at0071],
											3|[local::at0072],
											4|[local::at0073],
											5|[local::at0074]
										}
									}
									ELEMENT[at0064] occurrences matches {0..1} matches {    -- Ich lege wenig Wert darauf, wie viele Gegenstände manche Menschen als Zeichen des Erfolgs besitzen.
										value matches {
											1|[local::at0077],
											2|[local::at0078],
											3|[local::at0079],
											4|[local::at0080],
											5|[local::at0081]
										}
									}
									ELEMENT[at0075] occurrences matches {0..1} matches {    -- Ich versuche mir das Leben einfach zu machen, was Besitz angeht.
										value matches {
											1|[local::at0082],
											2|[local::at0083],
											3|[local::at0084],
											4|[local::at0085],
											5|[local::at0086]
										}
									}
									ELEMENT[at0076] occurrences matches {0..1} matches {    -- Es verschafft mir große Befriedigung, etwas zu kaufen.
										value matches {
											1|[local::at0088],
											2|[local::at0089],
											3|[local::at0090],
											4|[local::at0091],
											5|[local::at0092]
										}
									}
									ELEMENT[at0087] occurrences matches {0..1} matches {    -- Manchmal macht es mich sehr unzufrieden, dass ich mir nicht alles kaufen kann, was mir gefällt.
										value matches {
											1|[local::at0093],
											2|[local::at0094],
											3|[local::at0095],
											4|[local::at0096],
											5|[local::at0097]
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0098] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0101] occurrences matches {0..1} matches {    -- Kommentar
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}


