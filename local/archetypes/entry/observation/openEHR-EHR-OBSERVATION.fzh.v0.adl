definition
	OBSERVATION[at0000] matches {    -- German Compulsive Hoarding Inventory (FZH)
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {    -- Wie schwer fällt es Ihnen, etwas wegzuwerfen?
										value matches {
											1|[local::at0025],
											2|[local::at0026],
											3|[local::at0027],
											4|[local::at0028],
											5|[local::at0029]
										}
									}
									ELEMENT[at0005] occurrences matches {0..1} matches {    -- Wie sehr leiden Sie darunter, etwas wegzuwerfen?
										value matches {
											1|[local::at0030],
											2|[local::at0031],
											3|[local::at0032],
											4|[local::at0033],
											5|[local::at0034]
										}
									}
									ELEMENT[at0006] occurrences matches {0..1} matches {    -- Inwieweit haben Sie so viele Dinge, dass Ihr(e) Zimmer mit Dingen überladen/vollgestopft  sind?
										value matches {
											1|[local::at0035],
											2|[local::at0036],
											3|[local::at0037],
											4|[local::at0038],
											5|[local::at0039]
										}
									}
									ELEMENT[at0007] occurrences matches {0..1} matches {    -- Wie oft versuchen Sie zu vermeiden, etwas wegzuwerfen, weil es zu stressig ist oder zu  viel Zeit kostet?
										value matches {
											1|[local::at0040],
											2|[local::at0041],
											3|[local::at0042],
											4|[local::at0043],
											5|[local::at0044]
										}
									}
									ELEMENT[at0008] occurrences matches {0..1} matches {    -- Wie sehr würden Sie darunter leiden oder wie unangenehm wäre es für Sie, wenn Sie  etwas nicht kaufen könnten, was Sie wollten?
										value matches {
											1|[local::at0046],
											2|[local::at0047],
											3|[local::at0048],
											4|[local::at0049],
											5|[local::at0050]
										}
									}
									ELEMENT[at0009] occurrences matches {0..1} matches {    -- Inwieweit beeinträchtigen die vielen angesammelten Dinge in Ihrem Zuhause Ihr  Sozialleben, Ihr Berufsleben bzw. Ihren Alltag? Überlegen Sie, was Sie wegen der vielen  angesammelten Dinge bei Ihnen zu Hause nicht tun.
										value matches {
											1|[local::at0051],
											2|[local::at0052],
											3|[local::at0053],
											4|[local::at0054],
											5|[local::at0055]
										}
									}
									ELEMENT[at0010] occurrences matches {0..1} matches {    -- Wie oft fühlen Sie den Drang etwas zu erwerben, was Sie sehen (z.B. wenn Sie einkaufen  gehen oder wenn Ihnen etwas kostenlos angeboten wird)?
										value matches {
											1|[local::at0056],
											2|[local::at0057],
											3|[local::at0058],
											4|[local::at0059],
											5|[local::at0060]
										}
									}
									ELEMENT[at0013] occurrences matches {0..1} matches {    -- Wie stark ist Ihr Drang etwas zu kaufen oder zu erwerben, was Sie eigentlich nicht brauchen?
										value matches {
											1|[local::at0061],
											2|[local::at0062],
											3|[local::at0063],
											4|[local::at0064],
											5|[local::at0065]
										}
									}
									ELEMENT[at0014] occurrences matches {0..1} matches {    -- Wie viel Kontrolle haben Sie über Ihren Drang, Sachen zu erwerben?
										value matches {
											1|[local::at0066],
											2|[local::at0067],
											3|[local::at0068],
											4|[local::at0069],
											5|[local::at0070]
										}
									}
									ELEMENT[at0015] occurrences matches {0..1} matches {    -- Wie oft entscheiden Sie sich dafür, Dinge aufzuheben, die Sie nicht brauchen und für die Sie wenig Platz haben?
										value matches {
											1|[local::at0071],
											2|[local::at0072],
											3|[local::at0073],
											4|[local::at0074],
											5|[local::at0075]
										}
									}
									ELEMENT[at0016] occurrences matches {0..1} matches {    -- Inwieweit können Sie bestimmte Wohnbereiche nicht nutzen, weil sie mit Dingen überladen/vollgestopft sind?
										value matches {
											1|[local::at0076],
											2|[local::at0077],
											3|[local::at0078],
											4|[local::at0079],
											5|[local::at0080]
										}
									}
									ELEMENT[at0017] occurrences matches {0..1} matches {    -- Inwieweit leiden Sie darunter, dass Ihre Wohnbereiche mit Dingen überladen/vollgestopft sind?
										value matches {
											1|[local::at0081],
											2|[local::at0082],
											3|[local::at0083],
											4|[local::at0084],
											5|[local::at0085]
										}
									}
									ELEMENT[at0018] occurrences matches {0..1} matches {    -- Wie oft passiert es, dass Sie wegen der vielen angesammelten Dinge keinen Besuch nach Hause einladen?
										value matches {
											1|[local::at0086],
											2|[local::at0087],
											3|[local::at0088],
											4|[local::at0089],
											5|[local::at0090]
										}
									}
									ELEMENT[at0019] occurrences matches {0..1} matches {    -- Wie oft kaufen Sie tatsächlich (oder erwerben Sie kostenlos) Dinge, für die Sie keinen unmittelbaren Gebrauch oder Nutzen haben?
										value matches {
											1|[local::at0091],
											2|[local::at0092],
											3|[local::at0093],
											4|[local::at0094],
											5|[local::at0095]
										}
									}
									ELEMENT[at0020] occurrences matches {0..1} matches {    -- Wie stark ist Ihr Drang etwas aufzuheben, obwohl Sie wissen, dass Sie es nie nutzen werden?
										value matches {
											1|[local::at0096],
											2|[local::at0097],
											3|[local::at0098],
											4|[local::at0099],
											5|[local::at0100]
										}
									}
									ELEMENT[at0021] occurrences matches {0..1} matches {    -- Inwieweit kann man bei Ihnen zu Hause nur schwer durchgehen, weil alles mit Dingen überladen/vollgestopft ist?
										value matches {
											1|[local::at0101],
											2|[local::at0102],
											3|[local::at0103],
											4|[local::at0104],
											5|[local::at0105]
										}
									}
									ELEMENT[at0022] occurrences matches {0..1} matches {    -- Inwieweit können Sie bestimmte Wohnbereiche nicht für deren bestimmten Zweck nutzen, weil sie mit Dingen überladen/vollgestopft sind? (Zum Beispiel Kochen, Nutzung von Möbeln, Geschirrspülen, Saubermachen usw.)
										value matches {
											1|[local::at0106],
											2|[local::at0107],
											3|[local::at0108],
											4|[local::at0109],
											5|[local::at0110]
										}
									}
									ELEMENT[at0023] occurrences matches {0..1} matches {    -- Inwieweit fühlen Sie sich nicht in der Lage, mit den vielen angesammelten Dingen bei Ihnen zu Hause klar zu kommen?
										value matches {
											1|[local::at0111],
											2|[local::at0112],
											3|[local::at0113],
											4|[local::at0114],
											5|[local::at0115]
										}
									}
									ELEMENT[at0024] occurrences matches {0..1} matches {    -- Wie oft sind Sie nicht in der Lage, Dinge wegzuwerfen, die Sie gern loswerden möchten?
										value matches {
											1|[local::at0116],
											2|[local::at0117],
											3|[local::at0118],
											4|[local::at0119],
											5|[local::at0120]
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0011] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0012] occurrences matches {0..1} matches {    -- Kommentar
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}


