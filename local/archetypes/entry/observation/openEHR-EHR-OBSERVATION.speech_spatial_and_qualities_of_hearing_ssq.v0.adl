	openEHR-EHR-OBSERVATION.speech_spatial_and_qualities_of_hearing_ssq.v0

concept
	[at0000]	-- Speech, Spatial and Qualities of Hearing Questionnaire
language
	original_language = <[ISO_639-1::en-us]>
description
	original_author = <
		["name"] = <"Nils Hebbelmann">
		["organisation"] = <"Hearing for All - University of Oldenburg ">
		["email"] = <"nils.hebbelmann@uni-oldenburg.de">
		["date"] = <"2023-05-16">
	>
	details = <
		["en-us"] = <
			language = <[ISO_639-1::en-us]>
			purpose = <"Der Fragebogen dient dazu, das Spektrum der Hörfähigkeit über mehrere Bereichen hinweg zu erfassen und verschiedene Situation des Alltagslebens abzudecken. 
Die 49 Items des Fragebogens sind in drei Skalen unterteilt. Der Bereich des Sprachhörens zielt darauf ab, ein breites Spektrum an Sprach- und Hörkontexten abzudecken, mit denen die Person im täglichen Leben konfrontiert sein kann. Der Bereich Räumliches Hören befasst sich mit den klassischen und physikalischen Komponenten des räumlichen Hörvermögens in Bezug auf Richtung, Entfernung und Bewegung von Geräuschen. Der dritte Abschnitt über andere Qualitäten befasst sich mit weiteren Komponenten des Hörens wie Trennung, Erkennen, wahrgenommene Klarheit und Natürlichkeit von Alltagsgeräuschen sowie mit der Anstrengung beim Hören.">
			use = <"Aufzeichnung der Ergebnisse der Skala für Sprache, räumliches Hören und Hörqualität.">
			keywords = <"Krankenhaus, Psychiatrie, Sprache, räumliche, Qualität, Hören, räumliches Hören und Hörqualität, SSQ", ...>
			misuse = <"Darf nicht zur Erfassung anderer Fragebögen als des Fragebogens zur Sprache, räumliches Hören und Hörqualität in der vorgegebenen Form verwendet werden.">
		>
	>
	lifecycle_state = <"in_development">
	other_contributors = <"Nils Hebbelmann, Carl von Ossietzky Universität Oldenburg", ...>
	other_details = <
