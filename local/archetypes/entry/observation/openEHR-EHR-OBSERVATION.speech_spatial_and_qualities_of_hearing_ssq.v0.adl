				["at0034"] = <
					text = <"Räumliches Hören">
					description = <"">
				>
				["at0035"] = <
					text = <"Sie sind im Freien an einem Ihnen unbekannten Ort. Sie hören, dass jemand einen Rasenmäher benutzt, aber Sie sehen nicht, wo das ist. Können Sie sofort sagen, woher das Geräusch kommt?">
					description = <"">
				>
				["at0036"] = <
					text = <"Sie sitzen am Tisch oder in einer Besprechung mit mehreren Personen. Sie können nicht jeden sehen. Können Sie sagen, wo jemand sitzt, sobald er/sie zu sprechen beginnt?">
					description = <"">
				>
				["at0037"] = <
					text = <"Sie sitzen zwischen zwei Personen. Eine Person beginnt zu sprechen. Können Sie sofort sagen, ob es dir Person zu Ihrer Linken oder Rechten ist, ohne hinzuschauen?">
					description = <"">
				>
				["at0038"] = <
					text = <"Sie sind in einem Ihnen unbekannten Haus. Es ist ruhig. Sie hören eine Tür schlagen. Können Sie sofort sagen, woher das Geräusch kam?">
					description = <"">
				>
				["at0039"] = <
					text = <"Sie sind in einem Treppenhaus mit Stockwerken über und unter Ihnen. Sie hören Geräusche aus einem anderen Stockwerk. Können Sie problemlos sagen, woher das Geräusch kommt?">
					description = <"">
				>
				["at0040"] = <
					text = <"Sie sind im Freien. Ein Hund bellt. Können Sie sofort sagen, wo der Hund ist, ohne hinzuschauen?">
					description = <"">
				>
				["at0041"] = <
					text = <"Sie stehen auf dem Bürgersteig an einer belebten Straße. Können Sie sofort hören, aus welcher Richtung ein Bus oder Lastwagen kommt, bevor Sie ihn sehen?">
					description = <"">
				>
				["at0042"] = <
					text = <"Sie sind auf der Straße. Können Sie anhand der Stimme oder den Schritten einer Person sagen, wie weit sie entfernt ist?">
					description = <"">
				>
				["at0043"] = <
					text = <"Können Sie anhand des Geräusches sagen, wie weit ein Bus oder Lastwagen entfernt ist?">
					description = <"">
				>
				["at0044"] = <
					text = <"Können Sie anhand des Geräusches sagen, in welche Richtung sich ein Bus oder Lastwagen bewegt, z.B. von links nach rechts oder rechts nach links?">
					description = <"">
				>
				["at0045"] = <
					text = <"Können Sie anhand der Stimme oder den Schritten einer Person sagen, in welche Richtung sie sich bewegt, z.B. von links nach rechts oder rechts nach links?">
					description = <"">
				>
				["at0046"] = <
					text = <"Können Sie anhand der Stimme oder den Schritten einer Person sagen, ob sie Ihnen entgegenkommt oder sich entfernt?">
					description = <"">
				>
				["at0047"] = <
					text = <"Können Sie anhand des Geräusches sagen, ob ein Bus oder Lastwagen Ihnen entgegenkommt oder sich entfernt?">
					description = <"">
				>
				["at0048"] = <
					text = <"Klingen Geräusche, die Sie hören können, als wäre diese eher in Ihrem Kopf oder außerhalb in der Umwelt?">
					description = <"[0] - Innerhalb meines Kopfes
[1] - Außerhalb meines Kopfes/ Umwelt">
				>
				["at0049"] = <
					text = <"Sind Geräusche von Personen oder Dinge die Sie erst hören, aber nicht sofort sehen können, näher als erwartet, als wenn Sie diese sehen können?">
					description = <"[0] - Viel näher
[1] - Nicht näher">
				>
				["at0050"] = <
					text = <"Sind Geräusche von Personen oder Dinge die Sie erst hören, aber nicht sofort sehen können, weiter entfernt als erwartet, als wenn Sie diese können können?">
					description = <"[0] - Viel weiter
[1] - Nicht weiter">
				>
				["at0051"] = <
					text = <"Haben Sie den Eindruck, dass Geräusche genau dort sind, wo Sie sie erwarten?">
					description = <"">
				>
				["at0052"] = <
					text = <"Hörqualität">
					description = <"">
				>
				["at0053"] = <
					text = <"Stellen Sie sich vor, Sie hören zwei Geräusche gleichzeitig, z.B. Wasser, das in ein Becken einläuft und gleichzeitig spielt ein Radio. Haben Sie den Eindruck, dass diese Geräusche wie voneinander getrennt klingen?">
					description = <"">
				>
				["at0054"] = <
					text = <"Sie sind in einem Raum und aus dem Radio kommt Musik. Es spricht eine andere Person in dem Raum. Können Sie die Stimme als getrennt von der Musik hören?">
					description = <"">
				>
				["at0055"] = <
					text = <"Empfinden Sie es als einfach verschiedene Leute, die Sie kennen, anhand ihrer Stimme zu erkennen.">
					description = <"">
				>
				["at0056"] = <
					text = <"Empfinden Sie es als einfach Musikstücke zu unterscheiden, die Ihnen bekannt sind?">
					description = <"">
				>
				["at0057"] = <
					text = <"Können Sie den Unterschied zwischen den Geräuschen benennen, z.B. Auto oder Bus, kochendes Wasser in einem Topf oder Bratgeräusche in einer Pfanne?">
					description = <"">
				>
				["at0060"] = <
					text = <"Wenn Sie der Musik zuhören, können Sie erkennen, welche Instrumente spielen?">
					description = <"">
				>
				["at0061"] = <
					text = <"Wenn Sie der Musik zuhören, klingt sie klar und natürlich?">
					description = <"">
				>
				["at0062"] = <
					text = <"Klingen Alltagsgeräusche, die Sie leicht hören können, klar für Sie (nicht verschwommen)?">
					description = <"">
				>
				["at0063"] = <
					text = <"Wenn Sie mehr als ein Geräusch gleichzeitig hören, haben Sie den Eindruck, dass dies wie ein einziges Geräuschwirrwarr klingt?">
