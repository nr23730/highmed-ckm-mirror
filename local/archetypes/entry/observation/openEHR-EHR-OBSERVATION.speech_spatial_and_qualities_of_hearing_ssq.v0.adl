				>
				["at0067"] = <
					text = <"Wenn Sie als Fahrer im Auto fahren, können Sie problemlos verstehen, was eine andere Person sagt, die neben Ihnen sitzt?">
					description = <"">
				>
				["at0068"] = <
					text = <"Wenn Mitfahrer im Auto sind, können Sie problemlos verstehen, was der Fahrer sagt, der neben Ihnen sitzt?">
					description = <"">
				>
