				>
				["at0070"] = <
					text = <"Haben Alltagsgeräusche, die Sie hören, für Sie einen künstlichen oder unnatürlichen Klang?">
					description = <"">
				>
				["at0072"] = <
					text = <"Müssen Sie sich sehr konzentrieren, wenn Sie jemandem oder etwas zuhören?">
