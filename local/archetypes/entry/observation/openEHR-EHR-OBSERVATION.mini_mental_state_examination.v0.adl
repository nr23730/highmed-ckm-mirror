											}
											ELEMENT[at0090] occurrences matches {0..1} matches {	-- 18. 65/ S
												value matches {
													0|[local::at0097], 	-- Zahl/Buchstabe wurde nicht korrekt genannt.
													1|[local::at0098]  	-- Zahl/Buchstabe wurde korrekt genannt.
												}
											}
										}
									}
									CLUSTER[at0018] occurrences matches {0..1} matches {	-- IV. Erinnerungsfähigkeit
