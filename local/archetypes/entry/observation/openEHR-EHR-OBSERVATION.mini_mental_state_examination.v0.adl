												}
											}
											ELEMENT[at0054] occurrences matches {0..1} matches {	-- 6. Bundesland
												value matches {
													0|[local::at0065], 	-- Das Bundesland wurde nicht korrekt genannt
													1|[local::at0066]  	-- Das Bundesland wurde korrekt genannt.
												}
											}
											ELEMENT[at0055] occurrences matches {0..1} matches {	-- 7. Landkreis/ Stadt
												value matches {
