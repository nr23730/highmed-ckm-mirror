							ITEM_TREE[at0003] matches {	-- Baum
								items cardinality matches {0..*; unordered} matches {
									CLUSTER[at0004] occurrences matches {0..1} matches {	-- I. Orientierung
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0045] occurrences matches {0..1} matches {	-- 1. Datum
												value matches {
													0|[local::at0046], 	-- Aktuelles Datum wurde nicht genannt
													1|[local::at0047]  	-- Aktuelles Datum wurde genannt
												}
											}
											ELEMENT[at0048] occurrences matches {0..1} matches {	-- 2. Jahreszeit
												value matches {
													0|[local::at0050], 	-- Jahreszeit wurde nicht genannt
													1|[local::at0049]  	-- Jahreszeit wurde genannt
												}
