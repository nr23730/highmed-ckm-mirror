												}
											}
											ELEMENT[at0110] occurrences matches {0..1} matches {	-- 23. Bleistift/Kugelschreiber benennen
												value matches {
													0|[local::at0121], 	-- Untersuchte konnte Bleistift/ Kugelschreiber nicht benennen.
													1|[local::at0122]  	-- Untersuchte konnte Bleistift/ Kugelschreiber benennen
												}
											}
											ELEMENT[at0118] occurrences matches {0..1} matches {	-- 24. "Sie leiht ihm kein Geld mehr"
												value matches {
													0|[local::at0123], 	-- Untersuchte konnte Satz nicht wiederholen.
													1|[local::at0124]  	-- Untersuchte konnte Satz wiederholen.
												}
											}
											ELEMENT[at0117] occurrences matches {0..1} matches {	-- 25. Nehmen Sie bitte das Papier in die Hand
												value matches {
													0|[local::at0126], 	-- Der Untersuchte hat die Anweisung nicht ausgeführt.
													1|[local::at0125]  	-- Der Untersuchte hat die Anweisung ausgeführt.
												}
											}
											ELEMENT[at0116] occurrences matches {0..1} matches {	-- 26. Falten Sie es in der Mitte
												value matches {
													0|[local::at0127], 	-- Der Untersuchte hat die Anweisung nicht ausgeführt.
													1|[local::at0128]  	-- Der Untersuchte hat die Anweisung ausgeführt.
												}
											}
											ELEMENT[at0115] occurrences matches {0..1} matches {	-- 27. Lassen Sie es auf den Boden fallen
												value matches {
													0|[local::at0129], 	-- Der Untersuchte hat die Anweisung nicht ausgeführt
													1|[local::at0130]  	-- Der Untersuchte hat die Anweisung ausgeführt.
												}
											}
											ELEMENT[at0114] occurrences matches {0..1} matches {	-- 28. Schriftliche Anweisung "Bitte schließen Sie die Augen"
												value matches {
													0|[local::at0131], 	-- Der Untersuchte hat die schriftliche Anweisung nicht korrekt ausgeführt.
													1|[local::at0132]  	-- Der Untersuchte hat die schriftliche Anweisung ausgeführt.
												}
											}
											ELEMENT[at0113] occurrences matches {0..1} matches {	-- 29. Mündliche Anweisung "Schreiben Sie bitte irgendeinen Satz"
												value matches {
													0|[local::at0133], 	-- Der Untersuchte konnte keinen Satz bilden.
													1|[local::at0134]  	-- Der Untersuchte konnte einen Satz bilden.
												}
											}
											ELEMENT[at0112] occurrences matches {0..1} matches {	-- 30. Fünfecke nachzeichnen
												value matches {
													0|[local::at0135], 	-- Der Untersuchte konnte die Fünfecke nicht korrekt nachzeichnen.
													1|[local::at0136]  	-- Der Untersuchte konnte die Fünfecke korrekt nachzeichnen.
												}
											}
										}
									}
									ELEMENT[at0043] occurrences matches {0..1} matches {	-- Gesamtpunktzahl
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..30|}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0108] matches {	-- Baum
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0138] occurrences matches {0..*} matches {	-- Erweiterungen
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


