												}
											}
											ELEMENT[at0056] occurrences matches {0..1} matches {	-- 8. Stadt/ Stadtteil
												value matches {
													0|[local::at0069], 	-- Stadt/Stadtteil wurde nicht korrekt genannt.
													1|[local::at0070]  	-- Stadt/Stadtteil wurde korrekt genannt
												}
											}
											ELEMENT[at0057] occurrences matches {0..1} matches {	-- 9. Krankenhaus/ Pflegeheim
												value matches {
													0|[local::at0071], 	-- Krankenhaus/Pflegeeinrichtung wurde nicht korrekt genannt.
													1|[local::at0072]  	-- Krankenhaus/Pflegeeinrichtung wurde korrekt genannt.
												}
											}
											ELEMENT[at0058] occurrences matches {0..1} matches {	-- 10. Station/ Stockwerk
												value matches {
													0|[local::at0073], 	-- Station/ Stockwerk wurde nicht richtig genannt.
													1|[local::at0074]  	-- Station/ Stockwerk wurde richtig genannt
												}
											}
										}
									}
									CLUSTER[at0020] occurrences matches {0..1} matches {	-- II. Merkfähigkeit
