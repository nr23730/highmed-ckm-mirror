													1|[local::at0079]  	-- Begriff wurde wiederholt
												}
											}
											ELEMENT[at0076] occurrences matches {0..1} matches {	-- 12. Blume
												value matches {
													0|[local::at0080], 	-- Begriff wurde nicht wiederholt.
													1|[local::at0081]  	-- Begriff wurde wiederholt
												}
											}
											ELEMENT[at0077] occurrences matches {0..1} matches {	-- 13. Kerze
												value matches {
													0|[local::at0082], 	-- Begriff wurde nicht wiederholt.
													1|[local::at0083]  	-- Begriff wurde wiederholt
												}
