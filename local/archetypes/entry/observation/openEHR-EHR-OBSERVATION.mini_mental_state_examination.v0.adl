													1|[local::at0086]  	-- Zahl/Buchstabe wurde korrekt genannt.
												}
											}
											ELEMENT[at0087] occurrences matches {0..1} matches {	-- 15. 86/ H
												value matches {
													0|[local::at0091], 	-- Zahl/Buchstabe wurde nicht korrekt genannt.
													1|[local::at0092]  	-- Zahl/Buchstabe wurde korrekt genannt.
												}
											}
											ELEMENT[at0088] occurrences matches {0..1} matches {	-- 16. 79/ U
												value matches {
													0|[local::at0093], 	-- Zahl/Buchstabe wurde nicht korrekt genannt.
													1|[local::at0094]  	-- Zahl/Buchstabe wurde korrekt genannt.
												}
											}
											ELEMENT[at0089] occurrences matches {0..1} matches {	-- 17. 72/ T
