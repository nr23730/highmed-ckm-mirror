definition
	OBSERVATION[at0000] matches {	-- Test
		subject matches {
			PARTY_RELATED matches {
				relationship matches {
					DV_CODED_TEXT matches {
						defining_code matches {[openehr::41]}
					}
				}
			}
		}
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0012] occurrences matches {0..1} matches {	-- Jedes Ereignis
						data matches {
							ITEM_TABLE[at0003] matches {	-- Tabelle
								rotated matches {True}
								rows cardinality matches {0..1; unordered} matches {
									CLUSTER[at0013] occurrences matches {0..1} matches {	-- row
										items cardinality matches {1; unordered} matches {
											ELEMENT[at0004] matches {	-- Körpergröße
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::122]>
														list = <
															["1"] = <
																units = <"cm">
															>
														>
													>
												}
											}
										}
									}
								}
							}
						}
						state matches {
							ITEM_TREE[at0005] matches {	-- Baum
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0011] occurrences matches {0..*} matches {	-- Körpergewicht Personenstatus
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::124]>
												list = <
													["1"] = <
														units = <"kg">
													>
												>
											>
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0006] matches {	-- Baum
				items cardinality matches {1..*; unordered} matches {
					CLUSTER[at0014] occurrences matches {1..*} matches {	-- Körperdaten
						items cardinality matches {1..*; ordered} matches {
							ELEMENT[at0007] occurrences matches {0..*} matches {	-- Körpergröße Protokoll
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::122]>
										list = <
											["1"] = <
												units = <"cm">
											>
										>
									>
								}
							}
							ELEMENT[at0008] occurrences matches {0..*} matches {	-- Körpergewicht Protokoll
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::124]>
										list = <
											["1"] = <
												units = <"kg">
											>
										>
									>
								}
							}
						}
					}
					CLUSTER[at0016] occurrences matches {0..1} matches {	-- Herzfunktion
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0018] occurrences matches {0..*} matches {	-- Neues Element
								value matches {
									DV_DURATION matches {*}
								}
							}
						}
					}
				}
			}
		}
	}


