					text = <"Tree">
					description = <"@ internal @">
				>
				["at0007"] = <
					text = <"Anwendung">
					description = <"">
				>
				["at0037"] = <
					text = <"Für den Fall einer Situation des sogenannten Wachkomas treffe ich eine der folgenden Regelungen">
					description = <"">
				>
				["at0046"] = <
					text = <"Organspende">
					description = <"">
				>
				["at0047"] = <
					text = <"Es ist mir bewusst, dass Organe nur nach Feststellung des Hirntodes bei aufrechterhaltenem Kreislauf entnommen werden können. Deshalb gestatte ich ausnahmsweise für den Fall, dass bei mir eine Organspende medizinisch infrage kommt, die kurzfristige (Stunden bis höchstens wenige Tage umfassende) Durchführung intensivmedizinischer Maßnahmen zur Bestimmung des Hirntodes nach den Richtlinien der Bundesärztekammer und zur anschließenden Entnahme der Organe.">
					description = <"">
				>
				["at0051"] = <
					text = <"Beistand">
					description = <"">
				>
				["at0052"] = <
					text = <"Ich möchte Beistand durch">
					description = <"">
				>
				["at0053"] = <
					text = <"Folgende Person">
					description = <"">
				>
