															DV_TEXT matches {*}
														}
													}
													ELEMENT[at0022] occurrences matches {0..*} matches {    -- Given name
														value matches {
															DV_TEXT matches {*}
														}
													}
													ELEMENT[at0023] occurrences matches {0..*} matches {    -- Family name
														value matches {
															DV_TEXT matches {*}
														}
													}
													ELEMENT[at0024] occurrences matches {0..*} matches {    -- Suffix
														value matches {
															DV_TEXT matches {*}
														}
													}
												}
											}
											CLUSTER[at0025] occurrences matches {0..1} matches {    -- Institution
												items cardinality matches {1..*; unordered} matches {
													ELEMENT[at0026] occurrences matches {0..1} matches {    -- Name
														value matches {
															DV_TEXT matches {*}
														}
													}
													ELEMENT[at0027] occurrences matches {0..1} matches {    -- Contact
														value matches {
															DV_TEXT matches {*}
														}
													}
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}


