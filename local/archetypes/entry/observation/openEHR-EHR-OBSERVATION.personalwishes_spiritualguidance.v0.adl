									ELEMENT[at0004] occurrences matches {0..*} matches {    -- Wishes for personal well-being
										value matches {
											DV_TEXT matches {*}
										}
									}
									CLUSTER[at0005] occurrences matches {0..1} matches {    -- Pastoral/ Spiritual Support
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0006] occurrences matches {0..1} matches {    -- Support wanted in emergencies
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0007,    -- Yes
															at0008]    -- No
														}
													}
												}
											}
											ELEMENT[at0009] occurrences matches {0..1} matches {    -- Confession
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0010,    -- Roman Catholic
															at0011,    -- Protestant Lutheran
