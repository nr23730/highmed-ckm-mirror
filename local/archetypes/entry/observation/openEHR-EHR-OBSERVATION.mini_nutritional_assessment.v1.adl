				>
				["at0021"] = <
					text = <"Verlässt die Wohnung">
					description = <"Patient verlässt die Wohnung.">
				>
				["at0023"] = <
					text = <"Krankheit oder Stress">
					description = <"D Akute Krankheit oder psychischer Stress während der letzten 3 Monate?">
				>
				["at0024"] = <
					text = <"Ja">
