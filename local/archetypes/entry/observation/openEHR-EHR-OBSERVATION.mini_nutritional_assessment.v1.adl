					text = <"Gewichtsverlust > 3 kg">
					description = <"Mehr als 3 kg verloren.">
				>
				["at0015"] = <
					text = <"Nicht bekannt">
					description = <"Gewichtverlust ist nicht bekannt.">
				>
				["at0016"] = <
