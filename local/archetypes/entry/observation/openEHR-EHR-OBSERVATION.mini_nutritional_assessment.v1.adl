				>
				["at0029"] = <
					text = <"Keine psychologischen Probleme">
					description = <"Keine psychologischen Probleme bekannt.">
				>
				["at0031"] = <
					text = <"BMI">
					description = <"F1 Body Mass Index: Körpergewicht (kg) / Körpergröße²(m²).">
