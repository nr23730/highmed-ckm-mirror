	openEHR-EHR-OBSERVATION.mini_nutritional_assessment.v1

concept
	[at0000]	-- Mini Nutritional Assessment
language
	original_language = <[ISO_639-1::de]>
description
	original_author = <
		["name"] = <"Bianca Frenzel">
		["organisation"] = <"Medizinische Hochschule Hannover">
		["email"] = <"Frenzel.Bianca@mh-hannover.de">
