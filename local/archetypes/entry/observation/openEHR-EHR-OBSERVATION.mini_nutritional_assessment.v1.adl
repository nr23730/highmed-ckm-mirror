				>
				["at0034"] = <
					text = <"21< = BMI <23">
					description = <"BMI zwischen 21 und 23.">
				>
				["at0035"] = <
					text = <"Wadenumfang">
					description = <"F2 Wadenumfang (WU in cm).">
				>
				["at0036"] = <
					text = <"WU < 31 cm">
